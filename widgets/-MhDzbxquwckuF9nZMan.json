  "detailTemplate": "<b-card style=\"margin-top: 20px;\">\r\n  <div class=\"cohortCard\">\r\n\r\n    <div style=\"padding-top: 20px; padding-left: 20px; padding-right: 20px;\">\r\n      <span v-if=\"row.name\" class=\"company_title\" style=\"font-weight: bold; font-size: 25px; color:black; margin-left: 10px; \">{{row.name}}</span>\r\n    \r\n      <button\r\n        v-if=\"fbUser.email && row.programmePartnerOwner\" \r\n        @click=\"generateAuthLink_Cohort(row.rowKey)\"\r\n        :disabled=\"$getGlobalModel('generateAuthLink')\"  \r\n        style=\"float:right; \"\r\n      >\r\n      <span v-if=\"!$getGlobalModel('generateAuthLink')\">\r\n            Share with link \r\n            <i style=\"margin-left: 2px;\" class=\"fas fa-share\" />\r\n        </span>\r\n        <span v-else>\r\n            <b-spinner small />\r\n            <span>Wait...</span>\r\n        </span>\r\n      </button>\r\n\r\n      <button\r\n        v-if=\"fbUser.email && row.mentorOwner\" \r\n        @click=\"generateAuthLink_Cohort(row.rowKey)\"\r\n        :disabled=\"$getGlobalModel('generateAuthLink')\"  \r\n        style=\"float:right; \"\r\n      >\r\n      <span v-if=\"!$getGlobalModel('generateAuthLink')\">\r\n            Share with link \r\n            <i style=\"margin-left: 2px;\" class=\"fas fa-share\" />\r\n        </span>\r\n        <span v-else>\r\n            <b-spinner small />\r\n            <span>Wait...</span>\r\n        </span>\r\n      </button>\r\n    \r\n    </div>\r\n\r\n    <b-tabs card=\"\">\r\n\r\n      <b-tab title=\"Cohort Info\" :title-link-class=\"'tab-title-class'\" active=\"\">\r\n\r\n        <div class=\"row\">\r\n          <div class=\"column col-lg-12 col-sm-12 col-md-12\">\r\n            <p v-if=\"row.cohortSummary\" class=\"h4 font-weight-bold\"> Cohort Summary\r\n              <b-card-text>{{row.cohortSummary}}</b-card-text>\r\n            </p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row\">\r\n          <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n            <p v-if=\"row.createdDate\" class=\"h4 font-weight-bold\"> Created Date\r\n              <b-card-text>{{row.$createdDate$Date}}</b-card-text>\r\n            </p>\r\n          </div>\r\n        </div>\r\n\r\n\r\n\r\n\r\n      </b-tab>\r\n\r\n      <b-tab title=\"Cohort Owner\" :title-link-class=\"'tab-title-class'\">\r\n        <div class=\"row mentor\">\r\n          <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n            <p v-if=\"row.$mentorOwner\" class=\"h4 font-weight-bold\"> Cohort Owner (Mentor)\r\n              <center v-if=\"row.$mentorOwner\">\r\n                <b-card class=\"mcard\">\r\n                  <div class=\"card-body\" style=\"max-width: 100%;\">\r\n                    <div class=\"row\">\r\n\r\n                    </div>\r\n                    <center style=\"padding-top:20px;\"><img v-if=\"row.$mentorOwner.profileImage\" :src=\"$getDashboardModel('mentors')[row.mentorOwner].profileImage.url\" style=\"border-radius: 15px; max-width:100px  \" /> </center>\r\n                    <a href=\"#!\">\r\n                    </a>\r\n                    <div class=\"pt-4 text-center\">\r\n                      <h5 class=\"h3 title\">\r\n                        <span v-if=\"row.$mentorOwner && row.$mentorOwner.name && row.$mentorOwner.secondName\" class=\"d-block mb-1\">{{row.$mentorOwner.name}} {{row.$mentorOwner.secondName}} </span>\r\n                        <small id=\"about\" class=\"h4 font-weight-light \"></small>\r\n\r\n                        <div class=\"row\" style=\"justify-content:center;\" v-if=\"row.$mentorOwner && row.$mentorOwner.owner\">\r\n                          <small class=\"h4 font-weight-light \" v-for='e in row.$mentorOwner.$owner$display'>\r\n                            <a style=\"\" :href=\"'mailto:'+e\" target=\"_blank\">{{e}}</a>\r\n                          </small>\r\n                        </div>\r\n\r\n                      </h5>\r\n\r\n                      <div v-if=\"row.$mentorOwner && row.$mentorOwner.linkedInProfileLink\" class=\"row justify-content-center\">\r\n\r\n                        <a :href=\"row.$mentorOwner.linkedInProfileLink\" target=\"_blank\">\r\n                          <div v-if=\"row.$mentorOwner && row.$mentorOwner.linkedInProfileLink\" class=\"social-icon-contain\">\r\n                            <i v-if=\"row.$mentorOwner && row.$mentorOwner.linkedInProfileLink\" style=\"padding: 5px;\" class=\"fab fa-linkedin\"></i>\r\n                          </div>\r\n                        </a>\r\n\r\n                      </div>\r\n\r\n\r\n                    </div>\r\n                  </div>\r\n                </b-card>\r\n              </center>\r\n\r\n\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row pp\">\r\n          <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n            <p v-if=\"row.$programmePartnerOwner\" class=\"h4 font-weight-bold\"> Cohort Owner (Programme Partner)\r\n              <b-card>\r\n                <center v-if=\"row.$programmePartnerOwner\">\r\n                    <div class=\"card-body\" style=\"max-width: 100%;\">\r\n                      <div class=\"row\">\r\n\r\n                      </div>\r\n                      <center style=\"padding-top:20px;\"><img v-if=\"row.$programmePartnerOwner && row.$programmePartnerOwner.profileImage\" :src=\"$getDashboardModel('programmePartnerProfiles')[row.programmePartnerOwner].profileImage.url\" style=\"border-radius: 15px; max-width:100px  \" /> </center>\r\n                      <a href=\"#!\">\r\n                      </a>\r\n                      <div class=\"pt-4 text-center\">\r\n                        <h5 class=\"h3 title\">\r\n                          <span v-if=\"row.$programmePartnerOwner && row.$programmePartnerOwner.name && row.$programmePartnerOwner.secondName\" class=\"d-block mb-1\">{{row.$programmePartnerOwner.name}} {{row.$programmePartnerOwner.secondName}} </span>\r\n                          <small id=\"about\" class=\"h4 font-weight-light \"></small>\r\n\r\n                          <div class=\"row\" style=\"justify-content:center;\" v-if=\"row.$programmePartnerOwner && row.$programmePartnerOwner.owner\">\r\n                            <small class=\"h4 font-weight-light \" v-for='e in row.$programmePartnerOwner.$owner$display'>\r\n                              <a style=\"\" :href=\"'mailto:'+e\" target=\"_blank\">{{e}}</a>\r\n                            </small>\r\n                          </div>\r\n\r\n                        </h5>\r\n\r\n                        <div v-if=\"row.$programmePartnerOwner && row.$programmePartnerOwner.linkedInProfileLink\" class=\"row justify-content-center\">\r\n\r\n                          <a :href=\"row.$programmePartnerOwner.linkedInProfileLink\" target=\"_blank\">\r\n                            <div v-if=\"row.$programmePartnerOwner && row.$programmePartnerOwner.linkedInProfileLink\" class=\"social-icon-contain\">\r\n                              <i v-if=\"row.$programmePartnerOwner && row.$programmePartnerOwner.linkedInProfileLink\" style=\"padding: 5px; color: white !important;\" class=\"fab fa-linkedin\"></i>\r\n                            </div>\r\n                          </a>\r\n                        </div>\r\n\r\n                      </div>\r\n                    </div>\r\n                  </b-card>\r\n                </center>\r\n          </div>\r\n        </div>\r\n      </b-tab>\r\n\r\n<b-tab v-if=\"row.programmePartnerOwner \" title=\"Programme(s)\" :title-link-class=\"'tab-title-class'\">\r\n    \r\n        <h3 v-if=\"row.$programmes == null\" class=\"emptyMessage\"> There are no programmes currently assigned to this cohort.</h3>\r\n          \r\n          <div class=\"row\">\r\n            <div style=\"\" v-for=\"lookupProgrammes in row.$programmes\">\r\n              <b-card class=\"profileCard\" style=\"width: 300px; margin: 30px;\">\r\n                <div v-if=\"lookupProgrammes.programmeLogo\" style=\"height: 120px; margin-top: -10px;\">\r\n                    <img style=\"border-radius: 2px; \" class=\"profileImage\" v-if=\"lookupProgrammes.programmeLogo\" :src=\"$getDashboardModel('programmeData')[lookupProgrammes.rowKey].programmeLogo.url\"></img>\r\n                </div>\r\n\r\n                <div style=\"height: 30px;\" class=\"row \">\r\n                  <h2 v-if=\"lookupProgrammes.name\">{{truncateText(lookupProgrammes.name,20)}}</h2>\r\n                </div>\r\n\r\n              <div class=\"row2\" style=\"margin-top: 20px; justify-content: center;\">\r\n                <button @click='$setUser(\"selectedProgramme\", lookupProgrammes.rowKey); $dgShowEditRowModal(\"viewProgrammeModal\", \"-MljxbgKRAl2ix8bsDmK\")'>View Programme Details</button>\r\n              </div>\r\n\r\n              </b-card>\r\n            </div>\r\n          </div>\r\n\r\n      </b-tab>\r\n\r\n\r\n      <b-tab title=\"Indirect Investors/Mentor(s) Involved\" :title-link-class=\"'tab-title-class'\">\r\n\r\n        <h3 v-if=\"row.$mentors == null\" class=\"emptyMessage\"> There are currently no mentors in this cohort. </h3>\r\n\r\n        <div class=\"row\">\r\n          <div style=\"\" v-for=\"lookupMentor in row.$mentors\">\r\n\r\n            <b-card class=\"profileCard\" style=\"width: 300px; margin: 30px;\">\r\n              <center>\r\n                <div class=\"imageContainer\" style=\"height: 100px;\">\r\n                  <img v-if=\"lookupMentor.profileImage\" class=\"detail_image\" :src=\"$getDashboardModel('mentors')[lookupMentor.rowKey].profileImage.url\" style=\"border-radius: 15px; max-width:90px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n                </div>\r\n                <h3 style=\"margin-top: 5px;\" v-if=\"lookupMentor.name && lookupMentor.secondName\">{{lookupMentor.name}} {{lookupMentor.secondName}}</h3>\r\n\r\n                <div style=\"height: auto; padding-left: 10px; padding-right: 10px;\">\r\n                  <p v-if=\"lookupMentor.aboutMe\" class=\"h4 font-weight-light \"> {{truncateText(lookupMentor.aboutMe, 100)}} </p>\r\n                </div>\r\n\r\n                <div style=\"height: auto;\">\r\n                  <p v-if=\"lookupMentor.mainSectors\" class=\"h4 font-weight-bold\" style=\"font-size: 17px; font-weight: 400;\"> Main Sectors: </p>\r\n\r\n                  <div v-if=\"lookupMentor.mainSectors\">\r\n                    <p class=\"h4 font-weight-light\" v-for=\"item in lookupMentor.$mainSectors$display\">\r\n                      <span style=\"font-size: 1em;     font-weight: 400;\">\r\n                        <div class=\"rectangle\" style=\"padding: 2px; border-radius: 6px;\">\r\n                          {{item}}\r\n                        </div>\r\n                      </span>\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n\r\n                <div style=\"height: auto;\">\r\n                  <p v-if=\"lookupMentor.mainSkills\" class=\"h4 font-weight-bold\" style=\"color: black; font-size: 17px;\"> Main Skills: </p>\r\n\r\n                  <div v-if=\"lookupMentor.mainSkills\">\r\n                    <p class=\"h4 font-weight-light\" v-for=\"item in lookupMentor.$mainSkills$display\">\r\n                      <span style=\"font-size: 1em;     font-weight: 400;\">\r\n                        <div class=\"rectangle\" style=\"padding: 2px; border-radius: 6px;\">\r\n                          {{item}}\r\n                        </div>\r\n                      </span>\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n\r\n                <div style=\"height: 20px; margin-bottom: 20px;\" class=\"row justify-content-center\">\r\n                  <a :href=\"lookupMentor.linkedInProfileLink\" target=\"_blank\">\r\n                    <div v-if=\"lookupMentor.linkedInProfileLink\" class=\"social-icon-contain\">\r\n                      <i v-if=\"lookupMentor.linkedInProfileLink\" style=\"padding: 5px; bottom: 0;\" class=\"fab fa-linkedin\"></i>\r\n                    </div>\r\n                  </a>\r\n                </div>\r\n          </div>\r\n\r\n\r\n\r\n        </div>\r\n\r\n\r\n      </b-tab>\r\n\r\n      <b-tab title=\"Investor(s) Involved\" :title-link-class=\"'tab-title-class'\">\r\n\r\n        <h3 v-if=\"row.$investors == null\" class=\"emptyMessage\"> There are no investors in this cohort currently.</h3>\r\n\r\n        <div class=\"row\">\r\n          <div style=\"\" v-for=\"investorLookup in row.$investors\">\r\n\r\n            <b-card class=\"profileCard\" style=\"width: 300px; margin: 30px;\">\r\n              <center>\r\n                <div class=\"imageContainer\" style=\"height: 100px;\">\r\n                  <img v-if=\"investorLookup.profileImage\" class=\"detail_image\" :src=\"$getDashboardModel('investorProfiles')[investorLookup.rowKey].profileImage.url\" style=\"border-radius: 15px; max-width:90px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n                \r\n                    <a class=\"investorCompanyLogo\" v-if=\"investorLookup.rowKey && investorLookup.companyLogo\" @click=\"setActiveImage('InvestorCompanyLogo', investorLookup.rowKey)\">\r\n                    <img v-if=\"investorLookup.companyLogo\" class=\"profileImage \" :src=\"$getDashboardModel('investorProfiles')[investorLookup.rowKey].companyLogo.url\" style=\"border-radius: 15px; max-width:70px;  \">\r\n                        <span v-if=\"investorLookup.companyLogo && investorLookup.companyName\" class=\"tooltiptext\">Representing: {{investorLookup.companyName}}</span>    \r\n                    <img/> \r\n                  </a>\r\n                \r\n                </div>\r\n                <h3 v-if=\"investorLookup.name\">{{investorLookup.name}} {{investorLookup.secondName}}</h3>\r\n\r\n                <h3 v-if=\"$getUser('profileID') == investorLookup.rowKey\"> (You) </h3>\r\n\r\n                <div style=\"height: auto; padding: 10px; \">\r\n                  <span v-if=\"investorLookup.aboutMe\" class=\"\">{{truncateText(investorLookup.aboutMe,200)}}</span>\r\n                </div>\r\n\r\n                <div v-if=\"investorLookup.linkedInProfileLink\" class=\"row justify-content-center\">\r\n                  <a :href=\"investorLookup.linkedInProfileLink\" target=\"_blank\">\r\n                    <div v-if=\"investorLookup.linkedInProfileLink\" class=\"social-icon-contain\">\r\n                      <i v-if=\"investorLookup.linkedInProfileLink\" style=\"padding: 5px;\" class=\"fab fa-linkedin\"></i>\r\n                    </div>\r\n                  </a>\r\n                </div>\r\n\r\n              </center>\r\n            </b-card>\r\n\r\n          </div>\r\n        </div>\r\n\r\n      </b-tab>\r\n\r\n      <b-tab title=\"Companies Involved\" :title-link-class=\"'tab-title-class'\">\r\n\r\n        <h3 v-if=\"filterCohortCompaniesLength(row.rowKey) == 0\" class=\"emptyMessage\"> There are currently no companies in this cohort.</h3>\r\n\r\n        <div class=\"row\">\r\n          <div style=\"\" v-for=\"company in filterCohortCompanies(row.rowKey)\" :key=\"company.rowKey\">\r\n\r\n            <b-card class=\"profileCard\" style=\"width: 400px; margin: 30px;\">\r\n              <center>\r\n                <div class=\"imageContainer\" style=\"height: 100px;\">\r\n                  <img v-if=\"company.$company && company.$company.companyLogo\" class=\"detail_image\" :src=\"$getDashboardModel('companyProfiles')[company.company].companyLogo.url\" style=\"border-radius: 15px; max-width:90px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n                  <img v-if=\"company.$company && company.$companyUser.profileImage\" class=\"detail_image\" :src=\"$getDashboardModel('companyUserProfiles')[company.companyUser].profileImage.url\" style=\"border-radius: 15px; max-width:70px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n                </div>\r\n                <h3 style=\"margin-bottom: 10px;\" v-if=\"company.$company && company.$company.name\">{{company.$company.name}} & {{company.$companyUser.name}}</h3>\r\n\r\n                <div class=\"rectangle\" style=\"padding: 2px; border-radius: 6px; margin-top:-15px;\">\r\n                  <span v-if=\"company.$company && company.$company.fundraisingStage\"> {{company.$company.$fundraisingStage$display}} </span>\r\n                </div>\r\n\r\n                <div class=\"about\" style=\"height: auto;\">\r\n                  <p v-if=\"company.$company && company.$company.aboutTheCompany\" class=\"h4 font-weight-light\" style=\"padding-bottom: 30px;\">{{truncateText(company.$company.aboutTheCompany,200)}}</p>\r\n                </div>\r\n\r\n\r\n                <div class=\"row justify-content-center\">\r\n                  <div style=\"padding: 5px;\" v-if=\"company.$company && company.$company.website\">\r\n                    <a :href=\"'https://'+company.$company.website\" target=\"_blank\">\r\n                      <div class=\"social-icon-contain\">\r\n                        <i style=\"padding: 5px;\" class=\"fas fa-globe\"></i>\r\n                      </div>\r\n                    </a>\r\n                  </div>\r\n\r\n                  <div style=\"padding: 5px;\" v-if=\"company.$company && company.$company.linkedInLink\">\r\n                    <a :href=\"company.$company.linkedInLink\" target=\"_blank\">\r\n                      <div class=\"social-icon-contain\">\r\n                        <i style=\"padding: 5px;\" class=\"fab fa-linkedin\"></i>\r\n                      </div>\r\n                    </a>\r\n                  </div>\r\n                </div>\r\n\r\n\r\n                <button style=\"font-size: 15px; \" class=\"myButton application_button\" @click=\"$dgAddRowAndOpenModal('applications', {cohort: row.rowKey, name: 'Investor->CompanyUser', company: company.company, investor: $getUser('profileID'), companyUser: company.companyUser,  notificationDescription: `An Investor (in the same cohort as you) has applied to connect with ${company.$company.name}`})\">\r\n                  <break></break>\r\n                  Express Interest\r\n                </button>\r\n\r\n\r\n\r\n\r\n              </center>\r\n            </b-card>\r\n\r\n          </div>\r\n        </div>\r\n\r\n\r\n      </b-tab>\r\n\r\n\r\n    </b-tabs>\r\n  </div>\r\n</b-card>",
