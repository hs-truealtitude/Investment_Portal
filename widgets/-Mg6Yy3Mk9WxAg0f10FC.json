  "template": "<div style=\"\" class=\"cohortCard\">\r\n\r\n  <button style=\"top: 10px; position: absolute; left: 10px; width: 40px;\" class=\"myButton deleteCohort\" @click=\"removeCohort(row.rowKey)\">\r\n    <i class=\"fas fa-trash-alt\">\r\n      <span class=\"tooltiptext\">Delete Cohort</span>    \r\n    </i>\r\n    </button>\r\n  <button style=\"position: absolute; top: 10px; right: 10px; width: 40px;\" class=\"myButton editCohort\" @click=\"$dgShowEditRowModal('cohortData', row.rowKey);\">\r\n    <i class=\"fas fa-edit\"></i>\r\n    <span class=\"tooltiptext\">Edit Cohort</span>    \r\n  </button>\r\n\r\n  <center>\r\n\r\n    <div style=\"height: 100px;\" class=\"container\">\r\n      <a v-if=\"row.rowKey && row.$programmePartnerOwner.profileImage\" @click=\"setActiveImage('Cohort_PP', row.$programmePartnerOwner.rowKey)\" v-b-modal.image-modal style=\"cursor: pointer; height: 100px; display:inline-block; margin-top: 10px;\">\r\n        <img style=\"border-radius: 2px; \" class=\"profileImage\" v-if=\"row.$programmePartnerOwner && row.$programmePartnerOwner.profileImage\" :src=\"$getDashboardModel('programmePartnerProfiles')[row.programmePartnerOwner].profileImage.url\">\r\n      </a>\r\n\r\n       <div v-if=\"$getUser('unreadNotif') == row.rowKey\" class=\"dot\">\r\n          <span class=\"tooltiptext\">New User(s)</span>\r\n       </div>\r\n      </img>\r\n    </div>\r\n\r\n    <div class=\"titleContainer\" style=\"height: 30px; margin-top: 15px;\">\r\n      <h2 v-if=\"row.name\" style=\"\" class=\"title\">{{truncateText(row.name,20)}}</h2>\r\n    </div>\r\n\r\n    <div style=\"height: 170px; margin-top: 20px;\" class=\"about\">\r\n      <p style=\"\" v-if=\"row.cohortSummary\"> {{truncateText(row.cohortSummary,200)}} </p>\r\n    </div>\r\n\r\n<!-- More than 2 Programmes -->\r\n    <div v-if=\"countProgrammes(row.rowKey) > 2\" class=\"programmeCarousel\">  \r\n      <h3> Programme(s):</h3>\r\n      <div class=\"wrapper\">\r\n        <div class=\"slider\">\r\n          <div class=\"slide\">\r\n                <img v-if=\"lookupProgrammes.programmeLogo\" v-for=\"lookupProgrammes in row.$programmes\"  :src=\"$getDashboardModel('programmeData')[lookupProgrammes.rowKey].programmeLogo.url\" />\r\n                <img v-if=\"lookupProgrammes.programmeLogo\" v-for=\"lookupProgrammes in row.$programmes\" :src=\"$getDashboardModel('programmeData')[lookupProgrammes.rowKey].programmeLogo.url\" />\r\n                <img v-if=\"lookupProgrammes.programmeLogo\" v-for=\"lookupProgrammes in row.$programmes\" :src=\"$getDashboardModel('programmeData')[lookupProgrammes.rowKey].programmeLogo.url\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n<!-- Two Programmes -->\r\n    <div v-if=\"countProgrammes(row.rowKey) == 2\" class=\"programmeContainer\">  \r\n      <h3> Programme(s):</h3>\r\n          <div class=\"programmeLogo\">\r\n              <div class=\"logoContainer\" v-for=\"lookupProgrammes in row.$programmes\">\r\n                <img v-if=\"lookupProgrammes.programmeLogo\" style=\"width: 70px;\" :src=\"$getDashboardModel('programmeData')[lookupProgrammes.rowKey].programmeLogo.url\"/>\r\n              </div> \r\n          </div>\r\n    </div>\r\n\r\n<!-- Single Programme -->\r\n    <div v-if=\"countProgrammes(row.rowKey) == 1\" class=\"programmeContainer\">  \r\n      <h3> Programme(s):</h3>\r\n        <div v-for=\"lookupProgrammes in row.$programmes\" class=\"programmeLogo\">\r\n            <img v-if=\"lookupProgrammes.programmeLogo\" class=\"logoContainer\" style=\"width: 70px;\" :src=\"$getDashboardModel('programmeData')[lookupProgrammes.rowKey].programmeLogo.url\"/>\r\n            <span v-if=\"lookupProgrammes.name\" class=\"tooltiptext\">{{lookupProgrammes.name}}</span>\r\n        </div>\r\n    </div>\r\n\r\n<!-- No programmmes -->\r\n    <div v-if=\"countProgrammes(row.rowKey) < 1\" style=\"height: 165px;\">\r\n    </div>\r\n\r\n\r\n  <div>\r\n    <div class=\"row2\" style=\"justify-content: center;\">\r\n      <button @click='$setUser(\"activeCohortName\", row.name); $setUser(\"activeMyCohort\", row.rowKey); $setCurrentSubTab(\"-Mj-Q3txJ5KcUXArNJy5\",\"-MJq8S1suwASZ84EkCd5\");'>\r\n        <i style=\"padding-right: 5px;\" class=\"fas fa-chart-pie\"></i>\r\n        Metrics & Data\r\n      </button>\r\n    </div>\r\n\r\n    <div class=\"row2\" style=\"justify-content: center;\">\r\n      <button @click='openDetail(row.rowKey); $setUser(\"unreadNotif\", null); $setUser(\"activeMyCohort\", row.rowKey); '>View Cohort Details & Users Involved</button>\r\n    </div>\r\n\r\n    <!--div class=\"row inviteButtons\" style=\"justify-content: center;\">\r\n      <button @click=\"inviteInvestors(row.rowKey)\"><i class=\"fas fa-plus\"></i> Investors</button>\r\n      <button @click=\"inviteCompanies(row.rowKey)\"><i class=\"fas fa-plus\"></i> Companies</button>\r\n      <button @click=\"inviteMentors(row.rowKey)\"><i class=\"fas fa-plus\"></i> Indirect Investors</button>\r\n    </div-->\r\n\r\n    <div class=\"row2 inviteDropdown\" style=\"justify-content: center; margin-bottom: -25px;\">\r\n      <div class=\"dropdown \" style=\"width: 100%;\">\r\n        <button class=\"btn btn-secondary dropdown-toggle\" type=\"button\" id=\"customDropdownMenuButton\" data-toggle=\"dropdown\" aria-haspopup=\"true\" aria-expanded=\"false\">\r\n          <i class=\"fas fa-plus\"></i> <span> Invite to User Cohort </span>\r\n        </button>\r\n        <div class=\"dropdown-menu\" style=\"width: 100%;\" aria-labelledby=\"dropdownMenuButton\">\r\n          <a @click=\"inviteInvestors(row.rowKey)\" class=\"dropdown-item\" href=\"#\"><i class=\"fas fa-plus\"></i>Investors</a>\r\n          <a @click=\"inviteCompanies(row.rowKey)\" class=\"dropdown-item \" href=\"#\"><i class=\"fas fa-plus\"></i>Companies</a>\r\n          <a @click=\"inviteMentors(row.rowKey)\" class=\"dropdown-item none\" href=\"#\"><i class=\"fas fa-plus\"></i>Indirect Investors</a>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n  </center>\r\n</div>",
