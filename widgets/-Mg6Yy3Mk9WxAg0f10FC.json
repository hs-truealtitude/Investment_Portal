  "detailTemplate": "<b-card style=\"margin-top: 20px;\">\r\n  <div class=\"cohortCard\">\r\n\r\n    <div style=\"padding-top: 20px; padding-left: 20px; padding-right: 20px;\">\r\n      <span v-if=\"row.name\" class=\"company_title\" style=\"font-weight: bold; font-size: 25px; color:black; margin-left: 10px; \">{{row.name}}</span>\r\n   \r\n    <button\r\n        v-if=\"row.programmePartnerOwner == $getUser('profileID')\" \r\n        @click=\"generateAuthLink_Cohort(row.rowKey)\"\r\n        :disabled=\"$getGlobalModel('generateAuthLink')\"  \r\n        style=\"float:right; \"\r\n    >\r\n      <span v-if=\"!$getGlobalModel('generateAuthLink')\">\r\n            Share with link \r\n            <i style=\"margin-left: 2px;\" class=\"fas fa-share\" />\r\n        </span>\r\n        <span v-else>\r\n            <b-spinner small />\r\n            <span>Wait...</span>\r\n        </span>\r\n    </button>\r\n   \r\n    </div>\r\n\r\n    <b-tabs card=\"\">\r\n\r\n      <b-tab title=\"Cohort Info\" :title-link-class=\"'tab-title-class'\" active=\"\">\r\n\r\n        <div class=\"row\">\r\n          <div class=\"column col-lg-12 col-sm-12 col-md-12\">\r\n            <p v-if=\"row.cohortSummary\" class=\"h4 font-weight-bold\"> Cohort Summary\r\n              <b-card-text>{{row.cohortSummary}}</b-card-text>\r\n            </p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row\">\r\n          <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n            <p v-if=\"row.createdDate\" class=\"h4 font-weight-bold\"> Created Date\r\n              <b-card-text>{{row.$createdDate$Date}}</b-card-text>\r\n            </p>\r\n          </div>\r\n        </div>\r\n\r\n      </b-tab>\r\n\r\n  <b-tab v-if=\"row.programmePartnerOwner != $getUser('profileID')\" title=\"Cohort Owner\" :title-link-class=\"'tab-title-class'\">\r\n        <div class=\"row pp\">\r\n          <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n            <p v-if=\"row.$programmePartnerOwner\" class=\"h4 font-weight-bold\">Programme Partner\r\n\r\n              <center v-if=\"row.$programmePartnerOwner\">\r\n                <b-card class=\"mcard\">\r\n                  <div class=\"card-body\" style=\"max-width: 100%;\">\r\n                    <div class=\"row\">\r\n\r\n                    </div>\r\n                    <center style=\"padding-top:20px;\"><img v-if=\"row.$programmePartnerOwner && row.$programmePartnerOwner.profileImage\" :src=\"$getDashboardModel('programmePartnerProfiles')[row.programmePartnerOwner].profileImage.url\" style=\"border-radius: 15px; max-width:100px  \" /> </center>\r\n                    <a href=\"#!\">\r\n                    </a>\r\n\r\n                    <h3 style=\"margin-top:10px;\" v-if=\"row.$programmePartnerOwner && row.$programmePartnerOwner.name && row.$programmePartnerOwner.secondName\" class=\"d-block mb-1\">{{row.$programmePartnerOwner.name}} {{row.$programmePartnerOwner.secondName}} </h3>\r\n\r\n                    <div style=\"margin-bottom:10px;\" v-if=\"row.$programmePartnerOwner && row.$programmePartnerOwner.linkedInProfileLink\" class=\"row justify-content-center\">\r\n\r\n                      <a :href=\"row.$programmePartnerOwner.linkedInProfileLink\" target=\"_blank\">\r\n                        <div v-if=\"row.$programmePartnerOwner.linkedInProfileLink\" class=\"social-icon-contain\">\r\n                          <i v-if=\"row.$programmePartnerOwner.linkedInProfileLink\" style=\"padding: 5px; color: white !important;\" class=\"fab fa-linkedin\"></i>\r\n                        </div>\r\n                      </a>\r\n                    </div>\r\n\r\n                  </div>\r\n                </b-card>\r\n              </center>\r\n\r\n          </div>\r\n        </div>\r\n      </b-tab>\r\n\r\n      <b-tab title=\"Cohort Programme(s)\" :title-link-class=\"'tab-title-class'\">\r\n    \r\n        <h3 v-if=\"row.$programmes == null\" class=\"emptyMessage\"> There are no programmes currently assigned to this cohort.</h3>\r\n          \r\n          <div class=\"row\">\r\n            <div style=\"\" v-for=\"lookupProgrammes in row.$programmes\">\r\n              <b-card class=\"profileCard\" style=\"width: 300px; margin: 30px;\">\r\n\r\n                <div v-if=\"lookupProgrammes.programmeLogo\" style=\"height: 120px; margin-top: -10px;\">\r\n                    <img style=\"border-radius: 2px; \" class=\"profileImage\" v-if=\"lookupProgrammes.programmeLogo\" :src=\"$getDashboardModel('programmeData')[lookupProgrammes.rowKey].programmeLogo.url\"></img>\r\n                </div>\r\n\r\n                  <div v-if=\"lookupProgrammes.programmeLogo == null\" style=\"height: 120px; margin-top: -5px;\">\r\n              <i style=\"\" class=\"emptyPicture\" class=\"fas fa-user fa-4x\"></i>  \r\n                </div>\r\n\r\n                <div style=\"height: 30px;\" class=\"row \">\r\n                  <h2 v-if=\"lookupProgrammes.name\">{{truncateText(lookupProgrammes.name,20)}}</h2>\r\n                </div>\r\n\r\n                <div style=\"height: 205px; margin-top: 10px;\" class=\"buttonContainer\">\r\n                  <p v-if=\"lookupProgrammes.programmeSummary\">{{truncateText(lookupProgrammes.programmeSummary,200)}}</p>\r\n                </div>\r\n\r\n                <div style=\"height: 40px;\" class=\"row justify-content-center\">\r\n                  <h3 v-if=\"lookupProgrammes.fundSize\">Budget Size: Â£{{lookupProgrammes.fundSize |  numeralFormat('0,0')}}</h3>\r\n                </div>\r\n\r\n              </b-card>\r\n            </div>\r\n          </div>\r\n\r\n      </b-tab>\r\n\r\n      <b-tab title=\"Indirect Investor(s)/Mentor(s) Involved\" :title-link-class=\"'tab-title-class'\">\r\n\r\n        <h3 v-if=\"row.$mentors == null && row.programmePartnerOwner == $getUser('profileID')\" class=\"emptyMessage\"> You have no indirect investors in your cohort. Go back to the My Cohorts Page and click the invite button on your cohort card.</h3>\r\n        <h3 v-if=\"row.$mentors == null && row.programmePartnerOwner != $getUser('profileID')\" class=\"emptyMessage\"> There are no indirect investors in this cohort currently.</h3>\r\n\r\n\r\n        <div class=\"row\">\r\n          <div style=\"\" v-for=\"lookupMentor in row.$mentors\">\r\n\r\n            <b-card class=\"profileCard\" style=\"width: 300px; margin: 30px;\">\r\n              <button v-if=\"row.programmePartnerOwner == $getUser('profileID')\" style=\" font-size: 15px; position: absolute; left: 15px; top: 15px; width: 40px;\" class=\"myButton\" @click=\"removeMentor(lookupMentor.rowKey, row.rowKey)\"><i class=\"fas fa-user-slash\"></i></button>\r\n              </b-button>\r\n              <center>\r\n                <div class=\"imageContainer\" style=\"height: 100px;\">\r\n                  <img v-if=\"lookupMentor.profileImage\" class=\"detail_image\" :src=\"$getDashboardModel('mentors')[lookupMentor.rowKey].profileImage.url\" style=\"border-radius: 15px; max-width:90px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n                </div>\r\n                <h3 style=\"margin-top: 5px;\" v-if=\"lookupMentor.name\">{{lookupMentor.name}} {{lookupMentor.secondName}}</h3>\r\n\r\n                <button  v-if=\"row.programmePartnerOwner == $getUser('profileID')\" @click=\"$setUser('selectedMentor', lookupMentor.rowKey);$dgShowEditRowModal('viewProfileAndStatsModal', '-MltbCgz6IJjO3YC9gjX')\"> View Profile & Stats </button>\r\n\r\n              </center>\r\n            </b-card>\r\n\r\n          </div>\r\n\r\n        </div>\r\n\r\n      </b-tab>\r\n\r\n\r\n      <b-tab title=\"Investor(s) Involved\" :title-link-class=\"'tab-title-class'\">\r\n\r\n        <h3 v-if=\"row.$investors == null && row.programmePartnerOwner == $getUser('profileID')\" class=\"emptyMessage\"> You have no investors in your cohort. Go back to the My Cohorts Page and click the invite button on your cohort card.</h3>\r\n        <h3 v-if=\"row.$investors == null && row.programmePartnerOwner != $getUser('profileID')\" class=\"emptyMessage\"> There are no investors in this cohort currently.</h3>\r\n\r\n\r\n        <div class=\"row\">\r\n          <div style=\"\" v-for=\"investorLookup in row.$investors\">\r\n\r\n            <b-card class=\"profileCard\" style=\"width: 300px; margin: 30px;\">\r\n              <button  v-if=\"row.programmePartnerOwner == $getUser('profileID')\" style=\" font-size: 15px; position: absolute; left: 15px; top: 15px; width: 40px;\" class=\"myButton\" @click=\"removeInvestor(investorLookup.rowKey, row.rowKey)\"><i class=\"fas fa-user-slash\"></i></button>\r\n              </b-button>\r\n              <center>\r\n                <div class=\"imageContainer\" style=\"height: 100px;\">\r\n                  <img v-if=\"investorLookup.profileImage\" class=\"detail_image\" :src=\"$getDashboardModel('investorProfiles')[investorLookup.rowKey].profileImage.url\" style=\"border-radius: 15px; max-width:90px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n                </div>\r\n                <h3 v-if=\"investorLookup.name && investorLookup.secondName\">{{investorLookup.name}} {{investorLookup.secondName}}</h3>\r\n\r\n                <button  v-if=\"row.programmePartnerOwner == $getUser('profileID')\" @click=\"$setUser('selectedInvestor', investorLookup.rowKey);$dgShowEditRowModal('viewInvestorAndStatsModal', '-MoDDYeAIaeidjaAi2lX')\"> View Profile & Stats </button>\r\n\r\n                </h5>\r\n\r\n              </center>\r\n            </b-card>\r\n\r\n          </div>\r\n        </div>\r\n\r\n      </b-tab>\r\n\r\n\r\n      <b-tab title=\"Companies Involved\" :title-link-class=\"'tab-title-class'\">\r\n\r\n        <h3 v-if=\"filterCohortCompaniesLength(row.rowKey) == 0 && row.programmePartnerOwner == $getUser('profileID')\" class=\"emptyMessage\"> You have no companies in your cohort. Go back to the My Cohorts Page and click the invite button on your cohort card.</h3>\r\n        <h3 v-if=\"row.$companies == null && row.programmePartnerOwner != $getUser('profileID')\" class=\"emptyMessage\"> There are no companies in this cohort currently.</h3>\r\n\r\n        <div class=\"row\">\r\n\r\n          <div style=\"\" v-for=\"company in filterCohortCompanies(row.rowKey)\" :key=\"company.rowKey\">\r\n\r\n            <b-card v-if=\"company.$company\" class=\"profileCard\" style=\"width: 400px; margin: 30px;\">\r\n              <button  v-if=\"row.programmePartnerOwner == $getUser('profileID')\" style=\" font-size: 15px; position: absolute; left: 15px; top: 15px; width: 40px;\" class=\"myButton\" @click=\"removeCompanyUser(company.rowKey, company.companyUser, company.company, row.rowKey)\"><i class=\"fas fa-user-slash\"></i></button>\r\n              </b-button>\r\n              <center>\r\n                <div class=\"imageContainer\" style=\"height: 100px;\">\r\n                  <img v-if=\"company.$company && company.$company.companyLogo\" class=\"detail_image\" :src=\"$getDashboardModel('companyProfiles')[company.company].companyLogo.url\" style=\"border-radius: 15px; max-width:90px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n                  <img v-if=\"company.$companyUser && company.$companyUser.profileImage\" class=\"detail_image\" :src=\"$getDashboardModel('companyUserProfiles')[company.companyUser].profileImage.url\" style=\"border-radius: 15px; max-width:70px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n                </div>\r\n                <h2 v-if=\"company.$company && company.$company.name\">{{company.$company.name}} & {{company.$companyUser.name}}</h2>\r\n\r\n                <div class=\"rectangle\" style=\"padding: 2px; border-radius: 6px; margin-top:-10px;\">\r\n                  <span v-if=\"company.$company && company.$company.fundraisingStage\"> {{company.$company.$fundraisingStage$display}} </span>\r\n                </div>\r\n\r\n                <div class=\"about\" style=\"height: auto;\">\r\n                  <p v-if=\"company.$company && company.$company.aboutTheCompany\" class=\"h4 font-weight-light\" style=\"padding-bottom: 30px;\">{{truncateText(company.$company.aboutTheCompany,200)}}</p>\r\n                </div>\r\n\r\n              </center>\r\n            </b-card>\r\n\r\n          </div>\r\n        </div>\r\n\r\n        <div v-if=\"filterCohortCompaniesLength(row.rowKey) > 0 && $getUser('accountType') == 'Mentor'\">\r\n          <div class=\"row justify-content-center\" style=\"padding-bottom:1%;\">\r\n            <h1 style=\" color:black;\"> {{$getUser('activeCompanyName')}} & {{$getUser('activeCompanyOwnerName')}} {{$getUser('activeCompanyOwnerSecondName')}} </h1>\r\n          </div>\r\n\r\n          <b-card style=\"width: 100%;\">\r\n            <FullCalendar v-if='$getUser(\"filterCompanyUser\")' calendarId=\"-MVpai9ZhxoM_tJK6Q4D\" />\r\n          </b-card>\r\n        </div>\r\n\r\n      </b-tab>\r\n\r\n\r\n    </b-tabs>\r\n  </div>\r\n</b-card>",
