  "detailTemplate": "<b-card style=\"margin-top: 20px;\">\r\n  <div class=\"cohortCard\">\r\n\r\n  <div style=\"padding-top: 20px; padding-left: 20px; padding-right: 20px;\">\r\n    <img class=\"detail_image\" v-if=\"row.programmeLogo\" :src=\"row.programmeLogo.url\" width=\"80px\" height=\"80px\" style=\"margin-left: 5px; vertical-align:middle; border-radius: 15px;\">\r\n      <span v-if=\"row.name\" class=\"programme_title\" style=\"font-weight: bold; font-size: 25px; color:black; margin-left: 10px; \">{{row.name}} (Programme)</span>\r\n  </div>\r\n\r\n  <b-tabs card=\"\">\r\n\r\n    <b-tab title=\"Programme Info\" :title-link-class=\"'tab-title-class'\" active=\"\">\r\n\r\n      <div class=\"row\">\r\n        <div class=\"column col-lg-12 col-sm-12 col-md-12\">\r\n          <p v-if=\"row.programmeSummary\" class=\"h4 font-weight-bold\"> Programme Summary\r\n            <b-card-text>{{row.programmeSummary}}</b-card-text>\r\n          </p>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"row\">\r\n        <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n          <p v-if=\"row.fundSize\" class=\"h4 font-weight-bold\">Budget Size\r\n            <b-card-text>£{{row.fundSize |  numeralFormat('0,0')}}</b-card-text>\r\n          </p>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"row\">\r\n        <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n          <p v-if=\"row.createdDate\" class=\"h4 font-weight-bold\"> Created Date\r\n            <b-card-text>{{row.$createdDate$display}}</b-card-text>\r\n          </p>\r\n        </div>\r\n      </div>\r\n\r\n  </b-tab>\r\n\r\n  <b-tab title=\"Programme Owner\" :title-link-class=\"'tab-title-class'\">\r\n       <div class=\"row pp\">\r\n        <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n          <p v-if=\"row.$programmePartner\" class=\"h4 font-weight-bold\">Programme Partner\r\n            <b-card>\r\n              <center v-if=\"row.$programmePartner\">\r\n                  <div class=\"card-body\" style=\"max-width: 100%;\">\r\n                    <div class=\"row\">\r\n\r\n                    </div>\r\n                    <center style=\"padding-top:20px;\"><img v-if=\"row.$programmePartner && row.$programmePartner.profileImage\" :src=\"$getDashboardModel('programmePartnerProfiles')[row.programmePartner].profileImage.url\" style=\"border-radius: 15px; max-width:100px  \" /> </center>\r\n                    <a href=\"#!\">\r\n                    </a>\r\n                    <div class=\"pt-4 text-center\">\r\n                      <h5 class=\"h3 title\">\r\n                        <span v-if=\"row.$programmePartner && row.$programmePartner.name && row.$programmePartner.secondName\" class=\"d-block mb-1\">{{row.$programmePartner.name}} {{row.$programmePartner.secondName}} </span>\r\n                        <small id=\"about\" class=\"h4 font-weight-light \"></small>\r\n\r\n                        <div class=\"row\" style=\"justify-content:center;\" v-if=\"row.$programmePartner && row.$programmePartner.owner\">\r\n                          <small class=\"h4 font-weight-light \" v-for='e in row.$programmePartner.$owner$display'>\r\n                            <a style=\"\" :href=\"'mailto:'+e\" target=\"_blank\">{{e}}</a>\r\n                          </small>\r\n                        </div>\r\n\r\n                      </h5>\r\n\r\n                      <div v-if=\"row.$programmePartner && row.$programmePartner.linkedInProfileLink\" class=\"row justify-content-center\">\r\n\r\n                        <a :href=\"row.$programmePartner.linkedInProfileLink\" target=\"_blank\">\r\n                          <div v-if=\"row.$programmePartner.linkedInProfileLink\" class=\"social-icon-contain\">\r\n                            <i v-if=\"row.$programmePartner.linkedInProfileLink\" style=\"padding: 5px; color: white !important;\" class=\"fab fa-linkedin\"></i>\r\n                          </div>\r\n                        </a>\r\n                      </div>\r\n\r\n                    </div>\r\n                  </div>\r\n              </center>\r\n            </b-card>\r\n        </div>\r\n      </div>\r\n    </b-tab>\r\n\r\n  <b-tab title=\"Mentor(s) Involved\" :title-link-class=\"'tab-title-class'\">\r\n\r\n      <h3 v-if=\"row.$mentors == null\" class=\"emptyMessage\"> There are currently no mentors in this programme.</h3>\r\n\r\n       <div class=\"row\">\r\n        <div style=\"\" v-for=\"lookupMentor in row.$mentors\">\r\n\r\n          <b-card class=\"profileCard\" style=\"width: 300px; margin: 30px;\">\r\n            <center>\r\n              <div class=\"imageContainer\" style=\"height: 100px;\">\r\n                <img v-if=\"lookupMentor.profileImage\" class=\"detail_image\" :src=\"$getDashboardModel('mentors')[lookupMentor.rowKey].profileImage.url\" style=\"border-radius: 15px; max-width:90px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n              </div>\r\n               <h3 style=\"margin-top: 5px;\" v-if=\"lookupMentor.name\">{{lookupMentor.name}} {{lookupMentor.secondName}}</h3>\r\n\r\n              <div style=\"height: auto; padding-left: 10px; padding-right: 10px;\" >\r\n                <p v-if=\"lookupMentor.aboutMe\" class=\"h4 font-weight-light \"> {{truncateText(lookupMentor.aboutMe, 100)}} </p>\r\n              </div>\r\n\r\n              <div style=\"height: auto;\" >\r\n                    <p v-if=\"lookupMentor.mainSectors\" class=\"h4 font-weight-bold\" style=\"font-size: 17px;     font-weight: 400;\"> Main Sectors: </p>\r\n\r\n                    <div v-if=\"lookupMentor.mainSectors\">\r\n                      <p class=\"h4 font-weight-light\" v-for=\"item in lookupMentor.$mainSectors$display\">\r\n                        <span style=\"font-size: 1em;     font-weight: 400;\">\r\n                          <div class=\"rectangle\" style=\"padding: 2px; border-radius: 6px;\">\r\n                            {{item}}\r\n                          </div>\r\n                        </span>\r\n                      </p>\r\n                    </div>\r\n              </div>\r\n\r\n              <div style=\"height: auto;\" >\r\n                    <p v-if=\"lookupMentor.mainSkills\" class=\"h4 font-weight-bold\" style=\"color: black; font-size: 17px;\"> Main Skills: </p>\r\n\r\n                    <div v-if=\"lookupMentor.mainSkills\">\r\n                      <p class=\"h4 font-weight-light\" v-for=\"item in lookupMentor.$mainSkills$display\">\r\n                        <span style=\"font-size: 1em;     font-weight: 400;\">\r\n                          <div class=\"rectangle\" style=\"padding: 2px; border-radius: 6px;\">\r\n                            {{item}}\r\n                          </div>\r\n                        </span>\r\n                      </p>\r\n                    </div>\r\n              </div>\r\n\r\n              <div style=\"height: 20px; \" class=\"row justify-content-center\">\r\n                <a :href=\"lookupMentor.linkedInProfileLink\" target=\"_blank\">\r\n                  <div v-if=\"lookupMentor.linkedInProfileLink\" class=\"social-icon-contain\">\r\n                    <i v-if=\"lookupMentor.linkedInProfileLink\" style=\"padding: 5px; bottom: 0;\" class=\"fab fa-linkedin\"></i>\r\n                  </div>\r\n                </a>\r\n              </div>\r\n\r\n                <button style=\"width: 250px; margin-bottom: 10px;  margin-top: 20px;\" class=\"myButton application_button\" @click=\"$dgAddRowAndOpenModal('applications', {name: 'CompanyUser->Mentor(Programme)', programme:row.rowKey, mentor: lookupMentor.rowKey, companyUser:$getUser('profileID'),  notificationDescription: `A company user has applied for you to be their mentor (within a programme)`})\">\r\n                  Apply (in programme)\r\n                </button>\r\n\r\n            </center>\r\n          </b-card>\r\n\r\n\r\n    </b-tab>\r\n\r\n    <b-tab title=\"Investor(s) Involved\" :title-link-class=\"'tab-title-class'\" >\r\n\r\n      <h3 v-if=\"row.$investors == null\" class=\"emptyMessage\"> There are currently no investors in this programme.</h3>\r\n\r\n      <div class=\"row\">\r\n        <div style=\"\" v-for=\"investorLookup in row.$investors\">\r\n\r\n          <b-card class=\"profileCard\" style=\"width: 300px; margin: 30px;\">\r\n            <center>\r\n              <div class=\"imageContainer\" style=\"height: 100px;\">\r\n                <img v-if=\"investorLookup.profileImage\" class=\"detail_image\" :src=\"$getDashboardModel('investorProfiles')[investorLookup.rowKey].profileImage.url\" style=\"border-radius: 15px; max-width:90px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n              </div>\r\n              <h3 v-if=\"investorLookup.name\">{{investorLookup.name}} {{investorLookup.secondName}}</h3>\r\n             \r\n                <div style=\"height: auto; padding: 10px; \">\r\n\r\n                  <span v-if=\"investorLookup.aboutMe\" class=\"\">{{truncateText(investorLookup.aboutMe,200)}}</span>\r\n\r\n                </div>\r\n\r\n              </h5>\r\n\r\n              <div style=\"margin-top: 50px;\" class=\"row justify-content-center\">\r\n\r\n                <a  :href=\"investorLookup.linkedInProfileLink\" target=\"_blank\">\r\n                  <div v-if=\"investorLookup.linkedInProfileLink\" class=\"social-icon-contain\">\r\n                    <i v-if=\"investorLookup.linkedInProfileLink\" style=\"padding: 5px; \" class=\"fab fa-linkedin\"></i>\r\n                  </div>\r\n                </a>\r\n              </div>\r\n\r\n            </center>\r\n          </b-card>\r\n\r\n        </div>\r\n      </div>\r\n\r\n    </b-tab>\r\n\r\n    <b-tab title=\"My Companies Involved\" :title-link-class=\"'tab-title-class'\">\r\n\r\n      <h3 v-if=\"filterProgrammeMyCompaniesLength(row.rowKey) == 0\" class=\"emptyMessage\"> You don't have any companies involved in this programme.</h3>\r\n\r\n      <div class=\"row\">\r\n        <div style=\"\" v-for=\"company in filterProgrammeMyCompanies(row.rowKey)\" :key=\"company.rowKey\">\r\n\r\n          <b-card class=\"profileCard\" style=\"width: 400px; margin: 30px;\">\r\n            <center>\r\n              <div class=\"imageContainer\" style=\"height: 100px;\">\r\n                <img v-if=\"company.$company && company.$company.companyLogo\" class=\"detail_image\" :src=\"$getDashboardModel('companyProfiles')[company.company].companyLogo.url\" style=\"border-radius: 15px; max-width:90px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n              </div>\r\n              <h3 v-if=\"company.$company && company.$company.name\">{{company.$company.name}}</h3>\r\n\r\n              <div class=\"rectangle\" style=\"padding: 2px; border-radius: 6px;\">\r\n                <span v-if=\"company.$company && company.$company.fundraisingStage\"> {{company.$company.$fundraisingStage$display}} </span>\r\n              </div>\r\n\r\n              <div class=\"about\" style=\"height: auto;\">\r\n                <p v-if=\"company.$company && company.$company.aboutTheCompany\" class=\"h4 font-weight-light\" style=\"padding-bottom: 30px;\">{{truncateText(company.$company.aboutTheCompany,200)}}</p>\r\n              </div>\r\n\r\n              <div class=\"row justify-content-center\">\r\n                <div style=\"padding: 5px;\" v-if=\"company.$company && company.$company.website\">\r\n                  <a :href=\"'https://'+company.$company.website\" target=\"_blank\">\r\n                    <div class=\"social-icon-contain\">\r\n                      <i style=\"padding: 5px;\" class=\"fas fa-globe\"></i>\r\n                    </div>\r\n                  </a>\r\n                </div>\r\n\r\n                <div style=\"padding: 5px;\" v-if=\"company.$company && company.$company.linkedInLink\">\r\n                  <a :href=\"company.$company.linkedInLink\" target=\"_blank\">\r\n                    <div class=\"social-icon-contain\">\r\n                      <i style=\"padding: 5px;\" class=\"fab fa-linkedin\"></i>\r\n                    </div>\r\n                  </a>\r\n                </div>\r\n              </div>\r\n\r\n            </center>\r\n          </b-card>\r\n\r\n        </div>\r\n      </div>\r\n\r\n\r\n    </b-tab>\r\n\r\n\r\n    <b-tab title=\"Other Companies Involved\" :title-link-class=\"'tab-title-class'\" >\r\n\r\n     <h3 v-if=\"filterProgrammeCompaniesLength(row.rowKey) == 0\" class=\"emptyMessage\"> There are currently no other companies in this programme. </h3>\r\n\r\n      <div class=\"row\">\r\n\r\n        <div style=\"\" v-for=\"company in filterProgrammeCompanies(row.rowKey)\" :key=\"company.rowKey\">\r\n\r\n          <b-card v-if=\"company.$company\" class=\"profileCard\" style=\"width: 400px; margin: 30px;\">\r\n            <center>\r\n              <div class=\"imageContainer\" style=\"height: 100px;\">\r\n                <img v-if=\"company.$company && company.$company.companyLogo\" class=\"detail_image\" :src=\"$getDashboardModel('companyProfiles')[company.company].companyLogo.url\" style=\"border-radius: 15px; max-width:90px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n                <img v-if=\"company.$companyUser && company.$companyUser.profileImage\" class=\"detail_image\" :src=\"$getDashboardModel('companyUserProfiles')[company.companyUser].profileImage.url\" style=\"border-radius: 15px; max-width:70px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n              </div>\r\n              <p v-if=\"company.$company && company.$company.name\">{{company.$company.name}} & {{company.$companyUser.name}}</p>\r\n\r\n              <div class=\"rectangle\" style=\"padding: 2px; border-radius: 6px; margin-top:-20px;\">\r\n                <span v-if=\"company.$company && company.$company.fundraisingStage\"> {{company.$company.$fundraisingStage$display}} </span>\r\n              </div>\r\n\r\n              <div class=\"about\" style=\"height: auto;\">\r\n                <p v-if=\"company.$company && company.$company.aboutTheCompany\" class=\"h4 font-weight-light\" style=\"padding-bottom: 30px;\">{{truncateText(company.$company.aboutTheCompany,200)}}</p>\r\n              </div>\r\n\r\n            </center>\r\n          </b-card>\r\n\r\n        </div>\r\n      </div>\r\n\r\n    </b-tab>\r\n\r\n        <b-tab title=\"My Engagement Schedule\" :title-link-class=\"'tab-title-class'\" >\r\n\r\n  <h3 v-if=\"filterEngagementScheduleInProgrammeLength(row.rowKey) == 0\" class=\"emptyMessage\"> You don't have any sessions booked in this programme.</h3>\r\n\r\n      <div class=\"row\">\r\n        <div style=\"\" v-for=\"company in filterEngagementScheduleInProgramme(row.rowKey)\" :key=\"company.rowKey\">\r\n\r\n          <b-card class=\"profileCard\" style=\"width: 400px; margin: 30px;\">\r\n            \r\n          <button style=\"top: 10px; position: absolute; left: 10px; width: 40px;\" class=\"myButton\" @click=\"removeInProgrammeConnection(company.rowKey)\">\r\n            <center><i class=\"gg-close\"></i></center>\r\n          </button>\r\n            \r\n            <center>\r\n              <div class=\"imageContainer\" style=\"height: 100px;\">\r\n                <img v-if=\"company.$companyWithinProgramme && company.$companyWithinProgramme.companyLogo\" class=\"detail_image\" :src=\"$getDashboardModel('companyProfiles')[company.companyWithinProgramme].companyLogo.url\" style=\"border-radius: 15px; max-width:90px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n                <img v-if=\"company.$mentor && company.$mentor.profileImage\" class=\"detail_image\" :src=\"$getDashboardModel('mentors')[company.mentor].profileImage.url\" style=\"border-radius: 15px; max-width:70px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n              </div>\r\n              <h3 v-if=\"company.$mentor && company.$mentor.name && company.$mentor.secondName && company.$companyWithinProgramme.name\">{{company.$companyWithinProgramme.name}} <i class=\"fas fa-hands-helping\"></i> {{company.$mentor.name}} {{company.$mentor.secondName}}</h3>\r\n              <h3 v-if=\"company.$companyWithinProgramme && company.$companyWithinProgramme.name && company.programmePartnerOwner\">{{company.$companyWithinProgramme.name}} & {{company.$companyWithinProgrammeUser.name}}</h3>\r\n\r\n\r\n              <div class=\"rectangle\" style=\"padding: 2px; border-radius: 6px; margin-top:-10px;\">\r\n                <span v-if=\"company.$companyWithinProgramme && company.$companyWithinProgramme.fundraisingStage\"> {{company.$companyWithinProgramme.$fundraisingStage$display}} </span>\r\n              </div>\r\n\r\n              <div class=\"about\" style=\"height: auto;\">\r\n                <p v-if=\"company.$companyWithinProgramme && company.$companyWithinProgramme.aboutTheCompany\" class=\"h4 font-weight-light\" style=\"padding-bottom: 30px;\">{{truncateText(company.$companyWithinProgramme.aboutTheCompany,200)}}</p>\r\n              </div>\r\n\r\n              <div v-if=\"row.mentorOwner\" class=\"row\" style=\"justify-content:center;\" v-if=\"company.$companyWithinProgramme && company.$companyWithinProgramme.owner\">\r\n                <div style=\"padding-bottom: 10px;\">\r\n                  <button style=\" font-size: 15px;\" class=\"myButton\" @click='$setUser(\"filterCompanyUser\",company.$companyUser.rowKey);\r\n                        $setUser(\"filterCompany\",company.$companyWithinProgramme.rowKey);\r\n                        $setUser(\"filterMentor\", company.$mentor.rowKey);\r\n                        $setUser(\"activeMentorName\", company.$mentor.name);\r\n                        $setUser(\"activeMentorSecondName\", company.$mentor.secondName);\r\n                        $setUser(\"sessionDirection\", \"CompanyUser->Mentor\");     \r\n                        $setUser(\"activeCompanyName\", company.$companyWithinProgramme.name);\r\n                        $setUser(\"activeCompanyOwnerName\", company.$companyUser.name);\r\n                        $setUser(\"activeProgrammeRowKey\", row.rowKey);\r\n                        $setUser(\"mentorTimeAvailableNotes\", company.$mentor.timeAvailableNotes );\r\n                        $setUser(\"mentorDaysAvailable\", company.$mentor.daysAvailable )'>               \r\n                    Engagement Schedule\r\n                  </button>\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n            </center>\r\n          </b-card>\r\n        \r\n\r\n        </div>\r\n      </div>\r\n\r\n      <div v-if=\"filterEngagementScheduleInProgrammeLength(row.rowKey) > 0\">\r\n        <div v-if=\"$getUser('accountType') == 'CompanyUser'\">\r\n          <div class=\"row justify-content-center\" style=\"padding-bottom:1%;\">\r\n            <h1 style=\"color:black;\">  {{$getUser('activeCompanyName')}} & {{$getUser('activeMentorName')}} {{$getUser('activeMentorSecondName')}} Schedule </h1>\r\n          </div>\r\n        </div>\r\n\r\n           <center v-if=\"$getUser('mentorDaysAvailable') != null\">  <h2 style=\"text-decoration: underline;\">Preferred Days</h2> </center>\r\n\r\n           <div class=\"row justify-content-center\" style=\"padding-bottom:0%;\">\r\n          <h2 v-for=\"rows in getMentorDaysAvailable()\" style=\"color:black;\">\r\n             <div  v-for=\"item in rows.$daysAvailable$display\" >\r\n               <center>\r\n               <h3> {{item}} </h3>\r\n               </center>\r\n               </div>\r\n            </h2>\r\n        </div>\r\n\r\n        <div v-if=\"$getUser('mentorTimeAvailableNotes') != null\" class=\"row justify-content-center\" style=\"padding-bottom:0%;\">\r\n          <h3 style=\"color:black;\">{{$getUser('mentorTimeAvailableNotes')}}</h3>\r\n        </div>\r\n\r\n        <b-card v-if=\"filterEngagementScheduleInProgrammeLength(row.rowKey) > 0\">\r\n          <FullCalendar v-if='$getUser(\"filterCompanyUser\")' calendarId=\"-MVpai9ZhxoM_tJK6Q4D\" />\r\n        </b-card>\r\n      </div>\r\n\r\n\r\n    </b-tab>\r\n\r\n\r\n  </b-tabs>\r\n</div>\r\n</b-card>",
