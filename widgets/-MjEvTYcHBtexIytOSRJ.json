  "template": "<button style=\"top: 10px; position: absolute; left: 10px; width: 40px;\" class=\"myButton\" @click=\"leaveCohort(row.rowKey)\">\r\n  <center><i class=\"gg-close\"></i></center>\r\n</button>\r\n\r\n<div style=\"\" class=\"cohortCard\">\r\n\r\n  <center>\r\n\r\n  <div v-if=\"row.$programmePartnerOwner\" style=\"height: 100px;\">\r\n    <a v-if=\"row.$programmePartnerOwner && row.$programmePartnerOwner.profileImage\" @click=\"setActiveImage('Cohort_PP', row.$programmePartnerOwner.rowKey)\" v-b-modal.image-modal style=\"cursor: pointer; height: 100px; display:inline-block;\">     \r\n      <img style=\"border-radius: 2px; \" class=\"profileImage\" v-if=\"row.$programmePartnerOwner && row.$programmePartnerOwner.profileImage\" :src=\"$getDashboardModel('programmePartnerProfiles')[row.programmePartnerOwner].profileImage.url\">\r\n    </a>\r\n      <div v-if=\"$getUser('unreadNotif') == row.rowKey\" class=\"dot\">\r\n        <span class=\"tooltiptext\">New User(s)</span>\r\n       </div>\r\n      </img>\r\n    </div>\r\n\r\n  <div v-if=\"row.$mentorOwner\"  style=\"height: 100px;\">\r\n    <a v-if=\"row.$mentorOwner && row.$mentorOwner.profileImage\" @click=\"setActiveImage('Cohort_Mentor', row.$mentorOwner.rowKey)\" v-b-modal.image-modal style=\"cursor: pointer; height: 100px; display:inline-block;\">     \r\n      <img style=\"border-radius: 2px; \" class=\"profileImage\" v-if=\"row.$mentorOwner && row.$mentorOwner.profileImage\" :src=\"$getDashboardModel('mentors')[row.mentorOwner].profileImage.url\">\r\n    </a>\r\n      <div v-if=\"$getUser('unreadNotif') == row.rowKey\" class=\"dot\">\r\n        <span class=\"tooltiptext\">New User(s)</span>\r\n       </div>\r\n      </img>\r\n    </div>\r\n\r\n    <div style=\"height: 40px; margin-top: 10px; margin-bottom: 10px;\">\r\n      <h2 v-if=\"row.name\" style=\"margin-top: 10px;\" class=\"title\">{{truncateText(row.name,20)}}</h2>\r\n    </div>\r\n\r\n    <div style=\"height: 150px;\" class=\"buttonContainer\">\r\n      <p v-if=\"row.cohortSummary\"> {{truncateText(row.cohortSummary,200)}} </p>\r\n    </div>\r\n\r\n<!-- More than 2 Programmes -->\r\n    <div v-if=\"countProgrammes(row.rowKey) > 2\" class=\"programmeCarousel\">  \r\n      <h3> Programme(s):</h3>\r\n      <div class=\"wrapper\">\r\n        <div class=\"slider\">\r\n          <div class=\"slide\">\r\n                <img v-if=\"lookupProgrammes.programmeLogo\" v-for=\"lookupProgrammes in row.$programmes\"  :src=\"$getDashboardModel('programmeData')[lookupProgrammes.rowKey].programmeLogo.url\" />\r\n                <img v-if=\"lookupProgrammes.programmeLogo\" v-for=\"lookupProgrammes in row.$programmes\" :src=\"$getDashboardModel('programmeData')[lookupProgrammes.rowKey].programmeLogo.url\" />\r\n                <img v-if=\"lookupProgrammes.programmeLogo\" v-for=\"lookupProgrammes in row.$programmes\" :src=\"$getDashboardModel('programmeData')[lookupProgrammes.rowKey].programmeLogo.url\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n<!-- Two Programmes -->\r\n    <div v-if=\"countProgrammes(row.rowKey) == 2\" class=\"programmeContainer\">  \r\n      <h3> Programme(s):</h3>\r\n          <div class=\"programmeLogo\">\r\n              <div class=\"logoContainer\" v-for=\"lookupProgrammes in row.$programmes\">\r\n                <img v-if=\"lookupProgrammes.programmeLogo\" style=\"width: 70px;\" :src=\"$getDashboardModel('programmeData')[lookupProgrammes.rowKey].programmeLogo.url\"/>\r\n              </div> \r\n          </div>\r\n    </div>\r\n\r\n<!-- Single Programme -->\r\n    <div v-if=\"countProgrammes(row.rowKey) == 1\" class=\"programmeContainer\">  \r\n      <h3> Programme(s):</h3>\r\n        <div v-for=\"lookupProgrammes in row.$programmes\" class=\"programmeLogo\">\r\n            <img v-if=\"lookupProgrammes.programmeLogo\" class=\"logoContainer\" style=\"width: 70px;\" :src=\"$getDashboardModel('programmeData')[lookupProgrammes.rowKey].programmeLogo.url\"/>\r\n            <span v-if=\"lookupProgrammes.name\" class=\"tooltiptext\">{{lookupProgrammes.name}}</span>\r\n        </div>\r\n    </div>\r\n\r\n<!-- No programmmes -->\r\n    <div v-if=\"countProgrammes(row.rowKey) < 1\" style=\"height: 165px;\">\r\n    </div>\r\n\r\n    <div class=\"row2\" style=\"justify-content: center; margin-bottom: -30px;\">\r\n      <button @click='openDetail(row.rowKey); $setUser(\"activeCohort\", row.rowKey); $setUser(\"activeCohortRowKey\", row.rowKey); $setUser(\"activeProgrammeRowKey\", null); '>View Cohort Details & Users Involved</button>\r\n    </div>\r\n\r\n  </center>\r\n</div>",
