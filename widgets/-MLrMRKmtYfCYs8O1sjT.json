{
  "classes": "noscroll",
  "columnCount": 3,
  "columnMaxWidth": "350",
  "columnMinWidth": "350",
  "dataSource": "companyProfiles",
  "detailTemplate": "<div class=\"row\" style=\"margin-left: 20px; padding-top: 20px;\">\r\n\r\n  <div class=\"col-lg-12 col-sm-12 column\">\r\n    <div>\r\n      <b-card>\r\n\r\n        <div style=\"padding-top: 20px; padding-left: 20px; padding-right: 20px;\">\r\n          <img class=\"detail_image\" v-if=\"row.companyLogo\" :src=\"row.companyLogo.url\" width=\"100px\" height=\"100px\" style=\"margin-left: 5px; vertical-align:middle; border-radius: 15px;\">\r\n          <span class=\"company_title\" style=\"font-weight: bold; font-size: 25px; color:black; margin-left: 10px; \">{{row.name}} - {{row.dealProjectName}}</span>\r\n       \r\n          <button class=\"application_button\"\r\n            v-if=\"fbUser.email\" \r\n            @click=\"generateAuthLink_DetailedCompany(row.rowKey)\"\r\n            :disabled=\"$getGlobalModel('generateAuthLink')\"  \r\n            style=\"float:right; margin-right: 10px; \"\r\n          >\r\n          <span v-if=\"!$getGlobalModel('generateAuthLink')\">\r\n                Share detailed company profile \r\n                <i style=\"margin-left: 2px;\" class=\"fas fa-share\" />\r\n            </span>\r\n            <span v-else>\r\n                <b-spinner small />\r\n                <span>Wait...</span>\r\n            </span>\r\n          </button>\r\n       \r\n          <button class=\"application_button\"\r\n            v-if=\"fbUser.email\" \r\n            @click=\"generateAuthLink_Company(row.rowKey)\"\r\n            :disabled=\"$getGlobalModel('generateAuthLink')\"  \r\n            style=\"float:right; margin-right: 10px; \"\r\n          >\r\n          <span v-if=\"!$getGlobalModel('generateAuthLink')\">\r\n                Share simple company profile\r\n                <i style=\"margin-left: 2px;\" class=\"fas fa-share\" />\r\n            </span>\r\n            <span v-else>\r\n                <b-spinner small />\r\n                <span>Wait...</span>\r\n            </span>\r\n          </button>\r\n\r\n        </div>\r\n\r\n        <b-tabs card=\"\">\r\n          <b-tab title=\"Company Info\" :title-link-class=\"'tab-title-class'\" active=\"\">\r\n\r\n            <div class=\"details\">\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.name\" class=\"h4 font-weight-bold\"> Company Name\r\n                    <b-card-text>{{row.name}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.aboutTheCompany\" class=\"h4 font-weight-bold\"> About Company\r\n                    <b-card-text>{{row.aboutTheCompany}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.companyEmail\" class=\"h4 font-weight-bold\"> Email\r\n                    <b-card-text>{{row.companyEmail}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.mobileNumber\" class=\"h4 font-weight-bold\"> Contact Number\r\n                    <b-card-text>{{row.mobileNumber}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.website\" class=\"h4 font-weight-bold\"> Website\r\n                    <b-card-text>{{row.website}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.location\" class=\"h4 font-weight-bold\"> Location\r\n                    <b-card-text>{{row.$location$display}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.startDate\" class=\"h4 font-weight-bold\"> Company Start Date\r\n                    <b-card-text>{{row.$startDate$display}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.employeeAmount\" class=\"h4 font-weight-bold\"> Number of employees\r\n                    <b-card-text>{{row.employeeAmount}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.currentReturnOfRevenue\" class=\"h4 font-weight-bold\"> Current Return Of Revenue (Per Employee)\r\n                    <b-card-text>Â£{{row.currentReturnOfRevenue |  numeralFormat('0,0') }}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.numberOfJobsGenerated\" class=\"h4 font-weight-bold\"> Number of jobs generated in testbed to successfully deliver on program\r\n                    <b-card-text>{{row.numberOfJobsGenerated}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.predictedEmployeesHired1Year\" class=\"h4 font-weight-bold\"> Predicted Number Of Employees Hired (1 Year)\r\n                    <b-card-text>{{row.predictedEmployeesHired1Year}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.predictedEmployeesHired2Years\" class=\"h4 font-weight-bold\"> Predicted Number Of Employees Hired (2 Years)\r\n                    <b-card-text>{{row.predictedEmployeesHired2Years}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.predictedEmployeesHired3Years\" class=\"h4 font-weight-bold\"> Predicted Number Of Employees Hired (3 Year)\r\n                    <b-card-text>{{row.predictedEmployeesHired3Years}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.predictedEmployeesHired5Years\" class=\"h4 font-weight-bold\"> Predicted Number Of Employees Hired (5 Years)\r\n                    <b-card-text>{{row.predictedEmployeesHired5Years}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.predictedJobsPostProgram\" class=\"h4 font-weight-bold\"> Predicted Number Of Jobs Generated (During Program)\r\n                    <b-card-text>{{row.predictedJobsPostProgram}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.numberOfInteractionsCommercialOpportunities\" class=\"h4 font-weight-bold\"> Number of Commercial Opportunities\r\n                    <b-card-text>{{row.numberOfInteractionsCommercialOpportunities}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.productGrowthOtherIndustries\" class=\"h4 font-weight-bold\"> What other industries can the product or solution add value to? </p>\r\n                  <span style=\"font-size: 14px;\" v-for=\"item in row.$productGrowthOtherIndustries$display\">\r\n                    <div class=\"rectangle\" style=\"padding: 5px; border-radius: 6px; width: 250px; margin: 10px;\">\r\n                      {{item}}\r\n                    </div>\r\n                  </span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-tab>\r\n\r\n          <b-tab title=\"Company & Deal\" :title-link-class=\"'tab-title-class'\">\r\n            <div class=\"details\">\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.dealProjectName\" class=\"h4 font-weight-bold\"> Deal/Project Name\r\n                    <b-card-text>{{row.dealProjectName}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.companySummary\" class=\"h4 font-weight-bold\"> Company Summary\r\n                    <b-card-text>{{row.companySummary}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.commitmentClosingDate\" class=\"h4 font-weight-bold\"> Commitment Closing Date\r\n                    <b-card-text>{{row.$commitmentClosingDate$display}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.targetCompletionDate\" class=\"h4 font-weight-bold\"> Target Completion Date\r\n                    <b-card-text>{{row.$targetCompletionDate$display}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.capitalRaisedToDate\" class=\"h4 font-weight-bold\"> Capital raised to-date\r\n                    <b-card-text>Â£{{row.capitalRaisedToDate |  numeralFormat('0,0') }}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.preMoneyValuationOfCurrentRound\" class=\"h4 font-weight-bold\"> Pre-Money Valuation of current round\r\n                    <b-card-text>Â£{{row.preMoneyValuationOfCurrentRound |  numeralFormat('0,0') }}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.capitalAlreadyRaisedForCurrentFundingRound\" class=\"h4 font-weight-bold\"> Capital already raised/commited for the current funding round\r\n                    <b-card-text>Â£{{row.capitalAlreadyRaisedForCurrentFundingRound |  numeralFormat('0,0')}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.mainInvestorsCommitedInCurrentRound\" class=\"h4 font-weight-bold\"> Main investors commited/invested in current round\r\n                    <b-card-text>{{row.mainInvestorsCommitedInCurrentRound}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.minimumInvestment\" class=\"h4 font-weight-bold\"> Minimum Investment\r\n                    <b-card-text>Â£{{row.minimumInvestment |  numeralFormat('0,0')}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.targetReturnOfInvestment\" class=\"h4 font-weight-bold\"> Target Return of Investment\r\n                    <b-card-text>Â£{{row.targetReturnOfInvestment |  numeralFormat('0,0')}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.totalFundingRound\" class=\"h4 font-weight-bold\"> Total funding round\r\n                    <b-card-text>Â£{{row.totalFundingRound |  numeralFormat('0,0')}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.eISSEIS\" class=\"h4 font-weight-bold\"> EIS Eligible\r\n                    <b-card-text>{{row.$eISSEIS$display}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div v-if=\"row.investmentType\" class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.investmentType\" class=\"h4 font-weight-bold\"> Investment Type </p>\r\n                  <span style=\"color: black; font-size: 14px;\">\r\n                    <div class=\"rectangle\" style=\"padding: 5px; border-radius: 6px; width:70%; margin: 10px;\">\r\n                      {{row.$investmentType$display}}\r\n                    </div>\r\n                  </span>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.targetInvestorsForDealMarketing\" class=\"h4 font-weight-bold\"> Target Investors for Deal Marketing\r\n                  </p>\r\n                  <span style=\"font-size: 14px;\" v-for=\"item in row.$targetInvestorsForDealMarketing$display\">\r\n                    <div class=\"rectangle\" style=\"padding: 5px; border-radius: 6px; width:70%; margin: 10px;\">\r\n                      {{item}}\r\n                    </div>\r\n                  </span>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.specificStrategicValue\" class=\"h4 font-weight-bold\"> Specific strategic/value add investors\r\n                    <b-card-text>{{row.specificStrategicValue}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.excludedInvestors\" class=\"h4 font-weight-bold\"> Excluded Investors\r\n                    <b-card-text>{{row.excludedInvestors}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.companyDebt\" class=\"h4 font-weight-bold\"> Does the company have any debt?\r\n                    <b-card-text>{{row.companyDebt}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.litigationTaxOrPressure\" class=\"h4 font-weight-bold\"> Is the company subject to any litigation, tax or creditor pressure?\r\n                    <b-card-text>{{row.litigationTaxOrPressure}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.news\" class=\"h4 font-weight-bold\"> News\r\n                    <b-card-text>{{row.news}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <span style=\"font-size: 1em; font-weight: 400; \" v-for=\"member in filterBy($getGrid('news'), row.rowKey, 'company') \">\r\n                    <div class=\"rectangle\" style=\"padding: 5px; border-radius: 6px; width:auto; margin: 10px; \">\r\n\r\n                      <p style=\"padding-left: 10px; font-weight:bold; color:black;\">\r\n                        {{member.name}} <br>\r\n                        <span style=\"font-weight: normal;\">{{member.newsDescription}}</span> <br>\r\n                      <div style=\"margin-top:-10px; padding-left: 10px;\" v-if=\"member.newsLink\">\r\n                        <a :href=\"'https://'+member.newsLink\" target=\"_blank\">\r\n                          <div class=\"social-icon-contain\">\r\n                            <i style=\"padding: 5px; color: black !important;\" class=\"fas fa-globe\"></i>\r\n                          </div>\r\n                        </a>\r\n                      </div>\r\n                      </p>\r\n\r\n                    </div>\r\n                  </span>\r\n                </div>\r\n              </div>\r\n\r\n            </div>\r\n          </b-tab>\r\n\r\n          <b-tab v-if=\"row.sectors && row.skills && row.mentorRequirementsSummary\" title=\"Mentor Requirements\" :title-link-class=\"'tab-title-class'\">\r\n            <div class=\"details\">\r\n\r\n              <div v-if=\"row.mentorRequirementsSummary\" class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p class=\"h4 font-weight-bold\"> Mentor Requirements Summary </p>\r\n                  <b-card-text v-if=\"row.mentorRequirementsSummary\">{{row.mentorRequirementsSummary}}</b-card-text>\r\n                </div>\r\n              </div>\r\n              <p></p>\r\n\r\n              <div v-if=\"row.sectors\" class=\"row\">\r\n                <div v-if=\"row.sectors\" class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.sectors\" class=\"h4 font-weight-bold\"> Target Sectors </p>\r\n                  <span style=\"font-size: 14px;\" v-for=\"item in row.$sectors$display\">\r\n                    <div class=\"rectangle\" style=\"padding: 5px; border-radius: 6px; margin: 10px;\">\r\n                      {{item}}\r\n                    </div>\r\n                  </span>\r\n                </div>\r\n              </div>\r\n              <p> </p>\r\n              <div v-if=\"row.skills\" class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.skills\" class=\"h4 font-weight-bold\"> Target Skills </p>\r\n                  <span style=\"font-size: 14px;\" v-for=\"item in row.$skills$display\">\r\n                    <div class=\"rectangle\" style=\"padding: 5px; border-radius: 6px; margin: 10px;\">\r\n                      {{item}}\r\n                    </div>\r\n                  </span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-tab>\r\n\r\n          <b-tab title=\"HR Requirements\" :title-link-class=\"'tab-title-class'\">\r\n            <div class=\"details\">\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p class=\"h4 font-weight-bold\"> HR Requirements </p>\r\n                  <span style=\"font-size: 14px; \" v-for=\"req in filterBy($getGrid('hRRequirementsTable'), row.rowKey,  'companyProfiles') \">\r\n                    <div class=\"rectangle\" style=\"padding: 5px; border-radius: 6px; width:auto; margin: 10px; \">\r\n                      {{req.name}} - {{req.$requirementType$display}}\r\n                    </div>\r\n                  </span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-tab>\r\n\r\n          <b-tab title=\"Milestones\" :title-link-class=\"'tab-title-class'\">\r\n            <div class=\"details\">\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p class=\"h4 font-weight-bold\"> Milestones Completed (Last 6 Months)</p>\r\n                  <div class=\"row\">\r\n                    <div class=\"col-md-12 offset-md-3\">\r\n                      <ul class=\"rm timeline\" style=\"color: black;\">\r\n                        <li :class='\"timeRow\"' v-for=\"item in orderBy( filterBy($getGrid('milestones'), row.rowKey, 'companyProfiles'), 'targetCompletionDate')\">\r\n                          {{item.name}} <span style=\"font-weight: bold;\">{{item.$targetCompletionDate$display}}</span>\r\n                          <p>{{item.description}}</p>\r\n                        </li>\r\n                      </ul>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p class=\"h4 font-weight-bold\"> Milestones (Next Five Years)</p>\r\n                  <pre></pre>\r\n                  <div class=\"row\">\r\n                    <div class=\"col-md-12 offset-md-3\">\r\n                      <ul class=\"rm timeline\" style=\"color: black;\">\r\n                        <li :class='item.completed ? \"timeRow\" : \"timeRowRed\"' v-for=\"item in orderBy( filterBy($getGrid('nextFiveYearsMilestones'), row.rowKey, 'companyProfiles'), 'targetCompletionDate')\">\r\n                          {{item.name}} <span style=\"font-weight: bold;\">{{item.$targetCompletionDate$display}}</span>\r\n                          <p>{{item.description}}</p>\r\n                        </li>\r\n                      </ul>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-tab>\r\n\r\n          <b-tab title=\"Team\" :title-link-class=\"'tab-title-class'\">\r\n            <div class=\"details\">\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p class=\"h4 font-weight-bold\"> Team Members</p>\r\n                  <span style=\"font-size: 1em;     font-weight: 400; \" v-for=\"member in filterBy($getGrid('teamMembersTable'), row.rowKey, 'companyProfiles') \">\r\n                    <div class=\"rectangle\" style=\"padding: 5px; border-radius: 6px; width:auto; margin: 10px; \">\r\n                      <img v-if=\"member.profilePitcture\" :src=\"member.profilePitcture.url\" style=\"border-radius: 15px; vertical-align:middle; max-width:60px; margin-right: 10px;  \" />\r\n                      {{member.name}}\r\n                      {{member.secondName}}\r\n                      - {{member.$jobRole$display}}\r\n                      <a :href=\"member.linkedin\" target=\"_blank\">\r\n                        <i v-if=\"member.linkedin\" style=\"padding: 5px; float: right;\" class=\"fab fa-linkedin\"></i>\r\n                      </a>\r\n                    </div>\r\n                  </span>\r\n                </div>\r\n              </div>\r\n          </b-tab>\r\n\r\n          <b-tab title=\"References\" :title-link-class=\"'tab-title-class'\">\r\n            <div class=\"details\">\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.cornerstoneInvestorContactDetails\" class=\"h4 font-weight-bold\"> Cornerstone Investor\r\n                    <b-card-text>{{row.cornerstoneInvestorContactDetails}}</b-card-text>\r\n                  </p>\r\n                  <p class=\"h4 font-weight-bold\"> References</p>\r\