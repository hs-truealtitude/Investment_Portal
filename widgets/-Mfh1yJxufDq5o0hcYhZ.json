\r\n                    <div class=\"row\">\r\n\r\n                    </div>\r\n                    <center style=\"padding-top:20px;\"><img v-if=\"row.$programmePartnerOwner.profileImage && row.programmePartnerOwner\" :src=\"$getDashboardModel('programmePartnerProfiles')[row.programmePartnerOwner].profileImage.url\" style=\"border-radius: 15px; max-width:100px  \" /> </center>\r\n                    <a href=\"#!\">\r\n                    </a>\r\n                    <div class=\"pt-4 text-center\">\r\n                      <h5 class=\"h3 title\">\r\n                        <span v-if=\"row.programmePartnerOwner && row.$programmePartnerOwner.name && row.$programmePartnerOwner.secondName\" class=\"d-block mb-1\">{{row.$programmePartnerOwner.name}} {{row.$programmePartnerOwner.secondName}} </span>\r\n                        <small id=\"about\" class=\"h4 font-weight-light\"></small>\r\n\r\n                        <div class=\"row\" style=\"justify-content:center;\" v-if=\"row.programmePartnerOwner && row.$programmePartnerOwner.owner\">\r\n                          <small class=\"h4 font-weight-light\" v-for='e in row.$programmePartnerOwner.$owner$display'>\r\n                            <a style=\"\" :href=\"'mailto:'+e\" target=\"_blank\">{{e}}</a>\r\n                          </small>\r\n                        </div>\r\n\r\n                      </h5>\r\n\r\n                      <div v-if=\"row.programmePartnerOwner && row.$programmePartnerOwner.linkedInProfileLink\" class=\"row justify-content-center\">\r\n\r\n                        <a :href=\"row.$programmePartnerOwner.linkedInProfileLink\" target=\"_blank\">\r\n                          <div v-if=\"row.programmePartnerOwner && row.$programmePartnerOwner.linkedInProfileLink\" class=\"social-icon-contain\">\r\n                            <i v-if=\"row.programmePartnerOwner && row.$programmePartnerOwner.linkedInProfileLink\" style=\"padding: 5px; color: white !important;\" class=\"fab fa-linkedin\"></i>\r\n                          </div>\r\n                        </a>\r\n                      </div>\r\n\r\n                    </div>\r\n                  </div>\r\n                </b-card>\r\n              </center>\r\n\r\n          </div>\r\n        </div>\r\n      </b-tab>\r\n\r\n      <b-tab v-if=\"row.programmePartnerOwner \" title=\"Programme(s)\" :title-link-class=\"'tab-title-class'\">\r\n\r\n        <h3 v-if=\"row.$programmes == null\" class=\"emptyMessage\"> There are no programmes currently assigned to this cohort.</h3>\r\n\r\n        <div class=\"row\">\r\n          <div style=\"\" v-for=\"lookupProgrammes in row.$programmes\">\r\n            <b-card class=\"profileCard\" style=\"width: 300px; margin: 30px;\">\r\n              <div v-if=\"lookupProgrammes.programmeLogo\" style=\"height: 120px; margin-top: -10px;\">\r\n                <img style=\"border-radius: 2px; \" class=\"profileImage\" v-if=\"lookupProgrammes.programmeLogo\" :src=\"$getDashboardModel('programmeData')[lookupProgrammes.rowKey].programmeLogo.url\"></img>\r\n              </div>\r\n\r\n              <div style=\"height: 30px;\" class=\"row \">\r\n                <h2 v-if=\"lookupProgrammes.name\">{{truncateText(lookupProgrammes.name,20)}}</h2>\r\n              </div>\r\n\r\n              <div class=\"row2\" style=\"margin-top: 20px; justify-content: center;\">\r\n                <button @click='$setUser(\"selectedProgramme\", lookupProgrammes.rowKey); $dgShowEditRowModal(\"viewProgrammeModal\", \"-MljxbgKRAl2ix8bsDmK\")'>View Programme Details</button>\r\n              </div>\r\n\r\n            </b-card>\r\n          </div>\r\n        </div>\r\n\r\n      </b-tab>\r\n\r\n      <b-tab v-if=\"row.$mentorOwner == null\" title=\"Mentor(s) Involved\" :title-link-class=\"'tab-title-class'\">\r\n\r\n        <h3 v-if=\"row.$mentors == null\" class=\"emptyMessage\"> There are currently no mentors in this cohort.</h3>\r\n\r\n        <div class=\"row\">\r\n          <div style=\"\" v-for=\"lookupMentor in