  "detailTemplate": "<b-card style=\"margin-top: 20px;\">\r\n  <div class=\"cohortCard\">\r\n\r\n    <div style=\"padding-top: 20px; padding-left: 20px; padding-right: 20px;\">\r\n      <span v-if=\"row.name\" class=\"company_title\" style=\"font-weight: bold; font-size: 25px; color:black; margin-left: 10px; \">{{row.name}} (Cohort)</span>\r\n    </div>\r\n\r\n    <b-tabs card=\"\">\r\n\r\n      <b-tab title=\"Cohort Info\" :title-link-class=\"'tab-title-class'\" active=\"\">\r\n\r\n        <div class=\"row\">\r\n          <div class=\"column col-lg-12 col-sm-12 col-md-12\">\r\n            <p v-if=\"row.cohortSummary\" class=\"h4 font-weight-bold\"> Cohort Summary\r\n              <b-card-text v-if=\"row.cohortSummary\">{{row.cohortSummary}}</b-card-text>\r\n            </p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row\">\r\n          <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n            <p v-if=\"row.createdDate\" class=\"h4 font-weight-bold\"> Created Date\r\n              <b-card-text v-if=\"row.createdDate\">{{row.$createdDate$Date}}</b-card-text>\r\n            </p>\r\n          </div>\r\n        </div>\r\n      </b-tab>\r\n\r\n      <b-tab title=\"Cohort Owner\" :title-link-class=\"'tab-title-class'\">\r\n        <div class=\"row mentor\">\r\n          <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n            <p v-if=\"row.$mentorOwner\" class=\"h4 font-weight-bold\"> Cohort Owner (Mentor)\r\n              <center v-if=\"row.$mentorOwner\">\r\n                <b-card class=\"mcard\">\r\n                  <div class=\"card-body\" style=\"max-width: 100%;\">\r\n                    <div class=\"row\">\r\n\r\n                    </div>\r\n                    <center style=\"padding-top:20px;\"><img v-if=\"row.$mentorOwner && row.$mentorOwner.profileImage\" :src=\"$getDashboardModel('mentors')[row.mentorOwner].profileImage.url\" style=\"border-radius: 15px; max-width:100px  \" /> </center>\r\n                    <a hre