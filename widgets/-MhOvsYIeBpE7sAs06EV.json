  "detailTemplate": "<b-card style=\"margin-top: 20px;\">\r\n  <div class=\"cohortCard\">\r\n\r\n    <div style=\"padding-top: 20px; padding-left: 20px; padding-right: 20px;\">\r\n      <img class=\"detail_image\" v-if=\"row.programmeLogo\" :src=\"row.programmeLogo.url\" width=\"80px\" height=\"80px\" style=\"margin-left: 5px; vertical-align:middle; border-radius: 15px;\">\r\n      <span v-if=\"row.name\" class=\"programme_title\" style=\"font-weight: bold; font-size: 25px; color:black; margin-left: 10px; \">{{row.name}}</span>\r\n    </div>\r\n\r\n    <b-tabs card=\"\">\r\n\r\n      <b-tab title=\"Programme Info\" :title-link-class=\"'tab-title-class'\" active=\"\">\r\n\r\n        <div class=\"row\">\r\n          <div class=\"column col-lg-12 col-sm-12 col-md-12\">\r\n            <p v-if=\"row.programmeSummary\" class=\"h4 font-weight-bold\"> Programme Summary\r\n              <b-card-text>{{row.programmeSummary}}</b-card-text>\r\n            </p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row\">\r\n          <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n            <p v-if=\"row.fundSize\" class=\"h4 font-weight-bold\"> Fund Size\r\n              <b-card-text>£{{row.fundSize |  numeralFormat('0,0')}}</b-card-text>\r\n            </p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row\">\r\n          <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n            <p v-if=\"row.createdDate\" class=\"h4 font-weight-bold\"> Created Date\r\n              <b-card-text>{{row.$createdDate$display}}</b-card-text>\r\n            </p>\r\n          </div>\r\n        </div>\r\n\r\n      </b-tab>\r\n\r\n      <b-tab title=\"Programme Owner\" :title-link-class=\"'tab-title-class'\">\r\n        <div class=\"row pp\">\r\n          <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n            <p v-if=\"row.$programmePartner\" class=\"h4 font-weight-bold\">Programme Partner\r\n\r\n              <center v-if=\"row.$programmePartner\">\r\n                <b-card class=\"mcard\">\r\n                  <div class=\"card-body\" style=\"max-width: 100%;\">\r\n                    <div class=\"row\">\r\n\r\n                    </div>\r\n                    <center style=\"padding-top:20px;\"><img v-if=\"row.$programmePartner && row.$programmePartner.profileImage\" :src=\"$getDashboardModel('programmePartnerProfiles')[row.programmePartner].profileImage.url\" style=\"border-radius: 15px; max-width:100px  \" /> </center>\r\n                    <a href=\"#!\">\r\n                    </a>\r\n\r\n                    <h3 style=\"margin-top:10px;\" v-if=\"row.$programmePartner && row.$programmePartner.name && row.$programmePartner.secondName\" class=\"d-block mb-1\">{{row.$programmePartner.name}} {{row.$programmePartner.secondName}} </h3>\r\n\r\n\r\n\r\n                    <div style=\"margin-bottom:10px;\" v-if=\"row.$programmePartner && row.$programmePartner.linkedInProfileLink\" class=\"row justify-content-center\">\r\n\r\n                      <a :href=\"row.$programmePartner.linkedInProfileLink\" target=\"_blank\">\r\n                        <div v-if=\"row.$programmePartner.linkedInProfileLink\" class=\"social-icon-contain\">\r\n                          <i v-if=\"row.$programmePartner.linkedInProfileLink\" style=\"padding: 5px; color: white !important;\" class=\"fab fa-linkedin\"></i>\r\n                        </div>\r\n                      </a>\r\n                    </div>\r\n\r\n\r\n                  </div>\r\n                </b-card>\r\n              </center>\r\n\r\n          </div>\r\n        </div>\r\n      </b-tab>\r\n\r\n      <b-tab title=\"Mentor(s) Involved\" :title-link-class=\"'tab-title-class'\">\r\n\r\n        <h3 v-if=\"row.$mentors == null\" class=\"emptyMessage\"> You have no mentors in your programme. Go back to the My Programme page and click the invite button on your programme card.</h3>\r\n\r\n        <div class=\"row\">\r\n          <div style=\"\" v-for=\"lookupMentor in row.$mentors\">\r\n\r\n            <b-card class=\"profileCard\" style=\"width: 300px; margin: 30px;\">\r\n              <button style=\" font-size: 15px; position: absolute; left: 15px; top: 15px; width: 40px;\" class=\"myButton\" @click=\"removeMentorProgramme(lookupMentor.rowKey, row.rowKey)\"><i class=\"fas fa-user-slash\"></i></button>\r\n              </b-button>\r\n              <center>\r\n                <div class=\"imageContainer\" style=\"height: 100px;\">\r\n                  <img v-if=\"lookupMentor.profileImage\" class=\"detail_image\" :src=\"$getDashboardModel('mentors')[lookupMentor.rowKey].profileImage.url\" style=\"border-radius: 15px; max-width:90px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n                </div>\r\n                <h2 style=\"margin-top: 5px;\" v-if=\"lookupMentor.name\">{{lookupMentor.name}} {{lookupMentor.secondName}}</h2>\r\n\r\n                <div style=\"height: auto; padding-left: 10px; padding-right: 10px;\">\r\n                  <p v-if=\"lookupMentor.aboutMe\" class=\"\"> {{truncateText(lookupMentor.aboutMe, 100)}} </p>\r\n                </div>\r\n\r\n                <div class=\"wrap-collabsible\">\r\n                  <input id=\"collapsible\" class=\"toggle\" type=\"checkbox\">\r\n                  <label for=\"collapsible\" class=\"lbl-toggle\">Skills & Sectors</label>\r\n                  <div class=\"collapsible-content\">\r\n                    <div class=\"content-inner\">\r\n                      <p>\r\n                        <div style=\"height: auto;\">\r\n                            <p v-if=\"lookupMentor.mainSectors\" class=\"h4 font-weight-bold\" style=\"font-size: 17px;     font-weight: 400;\"> Main Sectors: </p>\r\n\r\n                            <div v-if=\"lookupMentor.mainSectors\">\r\n                              <p class=\"\" v-for=\"item in lookupMentor.$mainSectors$display\">\r\n                                <span style=\"font-size: 1em;     font-weight: 400;\">\r\n                                  <div class=\"rectangle\" style=\"padding: 2px; border-radius: 6px;\">\r\n                                    {{item}}\r\n                                  </div>\r\n                                </span>\r\n                              </p>\r\n                            </div>\r\n                        </div>\r\n\r\n                        <div style=\"height: auto;\">\r\n                          <p v-if=\"lookupMentor.mainSkills\" class=\"h4 font-weight-bold\" style=\"color: black; font-size: 17px;\"> Main Skills: </p>\r\n\r\n                          <div v-if=\"lookupMentor.mainSkills\">\r\n                            <p class=\"\" v-for=\"item in lookupMentor.$mainSkills$display\">\r\n                              <span style=\"font-size: 1em;     font-weight: 400;\">\r\n                                <div class=\"rectangle\" style=\"padding: 2px; border-radius: 6px;\">\r\n                                  {{item}}\r\n                                </div>\r\n                              </span>\r\n                            </p>\r\n                          </div>\r\n                        </div>\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n\r\n                <div class=\"row\" style=\"justify-content: center;\">\r\n                  <button style=\"\" @click='$setUser(\"selectedMentorName\", lookupMentor.name); $setUser(\"selectedMentorSecondName\", lookupMentor.secondName); $setUser(\"selectedMentor\", lookupMentor.rowKey); $dgShowEditRowModal(\"engagementStats\", \"-MkgmEsK7ynH6k96W7Gt\")'>\r\n                    Engagement Stats\r\n                  </button>\r\n                </div>\r\n\r\n                <div style=\"height: 30px;\" class=\"row justify-content-center\">\r\n                  <a :href=\"lookupMentor.linkedInProfileLink\" target=\"_blank\">\r\n                    <div v-if=\"lookupMentor.linkedInProfileLink\" class=\"social-icon-contain\">\r\n                      <i v-if=\"lookupMentor.linkedInProfileLink\" style=\"padding: 5px; bottom: 0;\" class=\"fab fa-linkedin\"></i>\r\n                    </div>\r\n                  </a>\r\n                </div>\r\n\r\n              </center>\r\n            </b-card>\r\n      </b-tab>\r\n\r\n      <b-tab title=\"Investor(s) Involved\" :title-link-class=\"'tab-title-class'\">\r\n\r\n        <h3 v-if=\"row.$investors == null\" class=\"emptyMessage\"> You have no investors in your programme. Go back to the My Programme page and click the invite button on your programme card.</h3>\r\n\r\n        <div class=\"row\">\r\n          <div style=\"\" v-for=\"investorLookup in row.$investors\">\r\n\r\n            <b-card class=\"profileCard\" style=\"width: 300px; margin: 30px;\">\r\n              <button style=\" font-size: 15px; position: absolute; left: 15px; top: 15px; width: 40px;\" class=\"myButton\" @click=\"removeInvestorProgramme(investorLookup.rowKey, row.rowKey)\"><i class=\"fas fa-user-slash\"></i></button>\r\n              </b-button>\r\n              <center>\r\n                <div class=\"imageContainer\" style=\"height: 100px;\">\r\n                  <img v-if=\"investorLookup.profileImage\" class=\"detail_image\" :src=\"$getDashboardModel('investorProfiles')[investorLookup.rowKey].profileImage.url\" style=\"border-radius: 15px; max-width:90px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n                </div>\r\n                <h3 v-if=\"investorLookup.name\">{{investorLookup.name}} {{investorLookup.secondName}}</h3>\r\n\r\n                <div style=\"height: auto; padding: 10px; \">\r\n\r\n                  <span v-if=\"investorLookup.aboutMe\" class=\"\">{{truncateText(investorLookup.aboutMe,200)}}</span>\r\n\r\n                </div>\r\n\r\n                </h5>\r\n\r\n                <div style=\"margin-top: 50px;\" class=\"row justify-content-center\">\r\n\r\n                  <a :href=\"investorLookup.linkedInProfileLink\" target=\"_blank\">\r\n                    <div v-if=\"investorLookup.linkedInProfileLink\" class=\"social-icon-contain\">\r\n                      <i v-if=\"investorLookup.linkedInProfileLink\" style=\"padding: 5px; \" class=\"fab fa-linkedin\"></i>\r\n                    </div>\r\n                  </a>\r\n                </div>\r\n\r\n\r\n\r\n                <div class=\"row\" style=\"margin-top: 10px; justify-content:center;\" v-if=\"investorLookup.owner\">\r\n                  <small class=\"\" v-for='e in investorLookup.$owner$display'>\r\n                    <a style=\"\" :href=\"'mailto:'+e\" target=\"_blank\">{{e}}</a>\r\n                  </small>\r\n                </div>\r\n              </center>\r\n            </b-card>\r\n\r\n          </div>\r\n        </div>\r\n\r\n      </b-tab>\r\n\r\n      <b-tab title=\"Companies Involved\" :title-link-class=\"'tab-title-class'\">\r\n\r\n        <h3 v-if=\"filterProgrammeCompaniesLength(row.rowKey) == 0\" class=\"emptyMessage\"> You have no companies in your programme. Go back to the My Cohorts Page and click the invite button on your cohort card.</h3>\r\n\r\n        <div class=\"row\">\r\n\r\n          <div style=\"\" v-for=\"company in filterProgrammeCompanies(row.rowKey)\" :key=\"company.rowKey\">\r\n\r\n            <b-card v-if=\"company.$company\" class=\"profileCard\" style=\"width: 400px; margin: 30px;\">\r\n              <button style=\" font-size: 15px; position: absolute; left: 15px; top: 15px; width: 40px;\" class=\"myButton\" @click=\"removeCompanyUser(company.rowKey)\"><i class=\"fas fa-user-slash\"></i></button>\r\n              </b-button>\r\n              <center>\r\n                <div class=\"imageContainer\" style=\"height: auto;\">\r\n                  <img v-if=\"company.$company && company.$company.companyLogo\" class=\"detail_image\" :src=\"$getDashboardModel('companyProfiles')[company.company].companyLogo.url\" style=\"border-radius: 15px; max-width:90px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n                  <img v-if=\"company.$companyUser && company.$companyUser.profileImage\" class=\"detail_image\" :src=\"$getDashboardModel('companyUserProfiles')[company.companyUser].profileImage.url\" style=\"border-radius: 15px; max-width:70px; padding: 5px; vertical-align:middle; margin-top: 10px;\" /></img>\r\n                </div>\r\n                <h3 v-if=\"company.$company && company.$company.name\">{{company.$company.name}} & {{company.$companyUser.name}}</h3>\r\n\r\n                <div class=\"rectangle\" style=\"padding: 2px; border-radius: 6px; margin-top:-10px;\">\r\n                  <span v-if=\"company.$company && company.$company.fundraisingStage\"> {{company.$company.$fundraisingStage$display}} </span>\r\n                </div>\r\n\r\n                <div class=\"about\" style=\"height: auto;\">\r\n                  <p v-if=\"company.$company && company.$company.aboutTheCompany\" class=\"\" style=\"padding-bottom: 30px;\">{{truncateText(company.$company.aboutTheCompany,200)}}</p>\r\n                </div>\r\n\r\n                <div class=\"row\" style=\"justify-content:center;\" v-if=\"company.$company && company.$company.owner\">\r\n                  <small class=\"\" v-for='e in company.$company.$owner$display'>\r\n                    <a style=\"\" :href=\"'mailto:'+e\" target=\"_blank\">{{e}}</a>\r\n                  </small>\r\n                </div>\r\n              </center>\r\n            </b-card>\r\n\r\n          </div>\r\n        </div>\r\n\r\n      </b-tab>\r\n\r\n\r\n    </b-tabs>\r\n  </div>\r\n</b-card>",
