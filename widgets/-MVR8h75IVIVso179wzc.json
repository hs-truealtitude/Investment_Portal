  "detailTemplate": "<div class=\"row\" style=\"margin-left: 20px; padding-top: 20px;\">\r\n  <div class=\"col-lg-12 col-sm-12 column\">\r\n    <div>\r\n      <b-card>\r\n\r\n        <div style=\"padding-top: 20px; padding-left: 20px; padding-right: 20px;\">\r\n          <img class=\"detail_image\" :src=\"$getDashboardModel('slingshotPitches')[row.slingshotPitches].companyLogo.url\" width=\"100px\" height=\"100px\" style=\"margin-left: 5px; vertical-align:middle; border-radius: 15px;\">\r\n          <span class=\"company_title\" style=\"font-weight: bold; font-family: 'Grenette pro', sans-serif; font-size: 25px; color:black; margin-left: 10px; \">{{row.$slingshotPitches.dealProjectName}} - {{row.$slingshotPitches.name}}</span>\r\n        </div>\r\n\r\n        <b-tabs card=\"\">\r\n          <b-tab title=\"Company Info\" :title-link-class=\"'tab-title-class'\" active=\"\">\r\n\r\n            <div class=\"details\">\r\n              <div class=\"row\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">                  \r\n                  <p v-if=\"row.$slingshotPitches.name\" class=\"h4 font-weight-bold\"> Company Name\r\n                    <b-card-text>{{row.$slingshotPitches.name}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">                  \r\n                  <p v-if=\"row.$slingshotPitches.aboutTheCompany\" class=\"h4 font-weight-bold\"> About Company\r\n                    <b-card-text>{{row.$slingshotPitches.aboutTheCompany}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"row\" v-if=\"filterInvestorApplications(row.$slingshotPitches.rowKey, $getUser('profileID'))\">\r\n                <div class=\"column col-lg-4 col-sm-12 col-md-12\">                  \r\n                  <p v-if=\"row.$slingshotPitches.companyEmail\" class=\"h4 font-weight-bold\"> Email\r\n         