  "detailTemplate": "<div class=\"row\" style=\"margin-left: 20px; padding-top: 20px;\">\r\n  <div class=\"col-lg-12 col-sm-12 column\">\r\n    <div>\r\n      <b-card>\r\n\r\n        <div style=\"padding-top: 20px; padding-left: 20px; padding-right: 20px;\">\r\n          <img v-if=\"row.$companyProfiles.companyLogo\" class=\"detail_image\" :src=\"$getDashboardModel('companyProfiles')[row.$companyProfiles.rowKey].companyLogo.url\" width=\"100px\" height=\"100px\" style=\"margin-left: 5px; vertical-align:middle; border-radius: 15px;\">\r\n          <span v-if=\"row.$companyProfiles.name\" class=\"company_title\" style=\"font-weight: bold; font-size: 25px; color:black; margin-left: 10px; \">{{row.$companyProfiles.name}} - {{row.$companyProfiles.dealProjectName}}</span>\r\n          <button style=\"float: right; font-size: 15px; \" class=\"myButton application_button\" @click=\"$dgAddRowAndOpenModal('applications', {name: 'Investor->CompanyUser', company:row.$companyProfiles.rowKey, investor: $getUser('profileID'), companyUser: row.$companyProfiles.companyUser,  notificationDescription: `An Investor has applied for ${row.$companyProfiles.name}`})\">\r\n            <break></break>\r\n            Express Interest\r\n          </button>\r\n        </div>\r\n\r\n        <b-tabs card=\"\">\r\n          <b-tab title=\"Company Info\" :title-link-class=\"'tab-title-class'\" active=\"\">\r\n\r\n            <div class=\"row\" style=\"padding-top: 10px;\">\r\n              <div class=\"column col-lg-12 col-sm-12 col-md-12\">\r\n                <p>\r\n                  <span style=\"color: black; font-size: 14px;\">\r\n                    <div class=\"rectangle\" style=\"background-color: #dfe3eb; padding: 5px; border-radius: 6px; width:70%; margin: 10px;\">\r\n                      Contact Details will only be displayed upon approval by the CompanyUser. Press the 'Express Interest' button to apply. </div>\r\n                  </span>\r\n                </p>\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"details\">\r\n              <div class=\"row\">\r\n                <div v-if=\"row.$companyProfiles.name\" class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.$companyProfiles.name\" class=\"h4 font-weight-bold\"> Company Name\r\n                    <b-card-text>{{row.$companyProfiles.name}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div v-if=\"row.$companyProfiles.aboutTheCompany\" class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.$companyProfiles.aboutTheCompany\" class=\"h4 font-weight-bold\"> About Company\r\n                    <b-card-text>{{row.$companyProfiles.aboutTheCompany}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"row\" v-if=\"filterInvestorApplications(row.$companyProfiles.rowKey, $getUser('profileID'))\">\r\n                <div v-if=\"row.$companyProfiles.companyEmail\" class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.$companyProfiles.companyEmail\" class=\"h4 font-weight-bold\"> Email\r\n                    <b-card-text>{{row.$companyProfiles.companyEmail}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div v-if=\"row.$companyProfiles.mobileNumber\" class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.$companyProfiles.mobileNumber\" class=\"h4 font-weight-bold\"> Contact Number\r\n                    <b-card-text>{{row.$companyProfiles.mobileNumber}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"row\">\r\n                <div v-if=\"row.$companyProfiles.website\" class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.$companyProfiles.website\" class=\"h4 font-weight-bold\"> Website\r\n                    <b-card-text>{{row.$companyProfiles.website}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div v-if=\"row.$companyProfiles.location\" class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.$companyProfiles.location\" class=\"h4 font-weight-bold\"> Location\r\n                    <b-card-text>{{row.$companyProfiles.$location$display}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div v-if=\"row.$companyProfiles.startDate\" class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.$companyProfiles.startDate\" class=\"h4 font-weight-bold\"> Company Start Date\r\n                    <b-card-text>{{row.$companyProfiles.$startDate$display}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div v-if=\"row.$companyProfiles.employeeAmount\" class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.$companyProfiles.employeeAmount\" class=\"h4 font-weight-bold\"> Number of employees\r\n                    <b-card-text>{{row.$companyProfiles.employeeAmount}}</b-card-text>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div v-if=\"row.$companyProfiles.currentReturnOfRevenue\" class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.$companyProfiles.currentReturnOfRevenue\" class=\"h4 font-weight-bold\"> Current Return Of Revenue (Per Employee)\r\n                    <b-card-text>Â£{{row.$companyProfiles.currentReturnOfRevenue |  numeralFormat('0,0') }}</b-card-text>\r\n                  </p>\r\n                </div>\r\n                <div v-if=\"row.$companyProfiles.numberOfJobsGenerated\" class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n                  <p v-if=\"row.$companyProfiles.numberOfJobsGenerated\" class=\"h4 font