                "isMultiple": false,
                "isAjax": false,
                "dataOptions": [],
                "ajaxDataUrl": "",
                "isChecked": false,
                "vif": "",
                "customCode": "<div class=\"cohortCard\" style=\"margin-top: -50px;\" v-if=\"getCohortMentor()\">\r\n\r\n  <div style=\"padding-left: 20px; padding-right: 20px; margin-bottom: 20px;\">\r\n    <img style=\"border-radius: 2px; width: 100px; \" class=\"profileImage\" v-if=\"getCohortMentor().profileImage\" :src=\"$getDashboardModel('mentors')[getCohortMentor().rowKey].profileImage.url\">    </img>\r\n    <span style=\"font-weight: bold; margin-left: 20px; font-size: 30px;\">{{ getCohortMentor().name }} {{getCohortMentor().secondName }}</span>\r\n  </div>\r\n\r\n  <b-tabs>\r\n\r\n    <b-tab title=\"Mentor Profile\" :title-link-class=\"'tab-title-class'\" active=\"\">\r\n      <div style=\"margin-top: 20px !important; \">\r\n\r\n        <div style=\"height: 20px; float:right;\" class=\"\">\r\n          <a :href=\"getCohortMentor().linkedInProfileLink\" target=\"_blank\">\r\n            <div v-if=\"getCohortMentor().linkedInProfileLink\" class=\"social-icon-contain\">\r\n              <i v-if=\"getCohortMentor().linkedInProfileLink\" style=\"padding: 5px; bottom: 0;\" class=\"fab fa-linkedin\"></i>\r\n            </div>\r\n          </a>\r\n        </div>\r\n\r\n\r\n        <p style=\"font-weight: bold;\">About Me:</p>\r\n        <p>{{ getCohortMentor().aboutMe }}</p>\r\n\r\n        <div v-if=\"getCohortMentor().location\" class=\"row\">\r\n          <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n            <p class=\"h4 font-weight-bold\"> Location:\r\n              <b-card-text>{{getCohortMentor().$location$display}}</b-card-text>\r\n            </p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row\">\r\n          <div v-if=\"getCohortMentor().mainSectors\" class=\"column col-lg-2 col-sm-12 col-md-12\">\r\n            <p class=\"h4 font-weight-bold\"> Main Sectors: </p>\r\n            <span style=\" font-size: 14px;\" v-for=\"item in getCohortMentor().$mainSectors$display\">\r\n              <div class=\"rectangle\" style=\"padding: 5px; border-radius: 6px; width: 250px; margin: 10px;\">\r\n                {{item}}\r\n              </div>\r\n            </span>\r\n          </div>\r\n          <div v-if=\"getCohortMentor().mainSkills\" class=\"column col-lg-2 col-sm-12 col-md-12\">\r\n            <p class=\"h4 font-weight-bold\"> Main Skills: </p>\r\n            <span style=\" font-size: 14px;\" v-for=\"item in getCohortMentor().$mainSkills$display\">\r\n              <div class=\"rectangle\" style=\"padding: 5px; border-radius: 6px; width: 250px; margin: 10px;\">\r\n                {{item}}\r\n              </div>\r\n            </span>\r\n\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row\">\r\n          <div v-if=\"getCohortMentor().sectors\" class=\"column col-lg-2 col-sm-12 col-md-12\">\r\n            <p class=\"h4 font-weight-bold\"> All Sectors: </p>\r\n            <span style=\" font-size: 14px;\" v-for=\"item in getCohortMentor().$sectors$display\">\r\n              <div class=\"rectangle\" style=\"padding: 5px; border-radius: 6px; width: 250px; margin: 10px;\">\r\n                {{item}}\r\n              </div>\r\n            </span>\r\n          </div>\r\n          <div v-if=\"getCohortMentor().skills\" class=\"column col-lg-2 col-sm-12 col-md-12\">\r\n            <p class=\"h4 font-weight-bold\"> All Skills: </p>\r\n            <span style=\" font-size: 14px;\" v-for=\"item in getCohortMentor().$skills$display\">\r\n              <div class=\"rectangle\" style=\"padding: 5px; border-radius: 6px; width: 250px; margin: 10px;\">\r\n                {{item}}\r\n              </div>\r\n            </span>\r\n          </div>\r\n        </div>\r\n\r\n        <div v-if=\"getCohortMentor().companyStage\" class=\"row\">\r\n          <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n            <p class=\"h4 font-weight-bold\"> Company stages I can help with: </p>\r\n            <span style=\" font-size: 14px;\" v-for=\"item in getCohortMentor().$companyStage$display\">\r\n              <div class=\"rectangle\" style=\"padding: 5px; border-radius: 6px; width: 250px; margin: 10px;\">\r\n                {{item}}\r\n              </div>\r\n            </span>\r\n\r\n          </div>\r\n        </div>\r\n\r\n        <div v-if=\"getCohortMentor().interests\" class=\"row\">\r\n          <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n            <p class=\"h4 font-weight-bold\"> Other sectors I'm interested in: </p>\r\n            <span style=\" font-size: 14px;\" v-for=\"item in getCohortMentor().$interests$display\">\r\n              <div class=\"rectangle\" style=\"padding: 5px; border-radius: 6px; width: 250px; margin: 10px;\">\r\n                {{item}}\r\n              </div>\r\n            </span>\r\n\r\n          </div>\r\n        </div>\r\n        </div>\r\n    </b-tab>\r\n\r\n    <b-tab title=\"Engagement Stats (within Cohort)\" :title-link-class=\"'tab-title-class'\">\r\n      <div style=\"margin-top: 20px !important;\" class=\"tabContainer\">\r\n      <div class=\"\" style=\"\">\r\n\r\n        <div class=\"row\" style=\"margin: 60px; color:black; \">\r\n\r\n          <div class=\"col\">\r\n            <center>\r\n              <i class=\"fas fa-user-clock fa-2x\"></i>\r\n              <h2>Sessions Booked</h2>\r\n              <h1>{{selectedMentorCountTotalSessions()}}</h1>\r\n            </center>\r\n          </div>\r\n\r\n          <div class=\"col\">\r\n            <center>\r\n              <i class=\"fas fa-hourglass-end fa-2x\"></i>\r\n              <h2>Total Session Hours</h2>\r\n              <h1>{{selectedMentorCountTotalSessionsHours()}} hours</h1>\r\n            </center>\r\n          </div>\r\n\r\n          <div class=\"col\">\r\n            <center>\r\n              <i class=\"fas fa-hand-holding fa-2x\"></i>\r\n              <h2>Initial Company Engagements</h2>\r\n              <h1>{{selectedMentorCountCompanyEngagements()}}</h1>\r\n            </center>\r\n          </div>\r\n\r\n        </div>\r\n\r\n      </div>\r\n      </div>\r\n\r\n    </b-tab>\r\n\r\n  </b-tabs>\r\n\r\n</div>\r\n",
