                "ajaxDataUrl": "",
                "isChecked": false,
                "vif": "",
                "customCode": "<div class=\"cohortData\">\r\n  <center>\r\n    <h1 style=\"text-decoration: underline;   font-size: 1.8em; \">Cohort Stats</h1>\r\n  </center>\r\n\r\n  <div class=\"row\" style=\"margin-top: 20px; color:black; \">\r\n\r\n    <div v-if=\"$getUser('accountType') == 'ProgrammePartner'\" class=\"col\">\r\n      <center>\r\n        <i class=\"fas fa-hands-helping fa-2x\"></i>\r\n        <h2> Mentor Initial Engagements </h2>\r\n        <h2>(with Companies)</h2>\r\n        <h1 style=\"color:black;\">{{cohortCountMentorApproaches()}}</h1>\r\n      </center>\r\n    </div>\r\n\r\n    <div v-if=\"$getUser('accountType') == 'ProgrammePartner'\" class=\"col\">\r\n      <center>\r\n        <i class=\"fas fa-hands-helping fa-2x\"></i>\r\n        <h2> Company Initial Engagements</h2>\r\n        <h2> (with Mentors) </h2>\r\n        <h1 style=\"\">{{cohortCountCompanyApproaches()}}</h1>\r\n      </center>\r\n    </div>\r\n\r\n    <div class=\"col\">\r\n      <center>\r\n        <i class=\"fas fa-user-check fa-2x\"></i><i style=\"margin-left: -2px;\" class=\"fas fa-user fa-2x\"></i>\r\n        <h2> Sessions Booked</h2>\r\n        <h2> (Mentor & Companies)</h2>\r\n        <h1 style=\"\">{{cohortCountTotalBookedSessions()}}</h1>\r\n      </center>\r\n    </div>\r\n\r\n    <div class=\"col\">\r\n      <center>\r\n        <i class=\"far fa-handshake fa-2x\"></i>\r\n\r\n        <h2> Investor Engagements </h2>\r\n        <h2>(with Companies)</h2>\r\n        <h1 style=\"color:black;\">{{cohortCountInvestorsApproaches()}}</h1>\r\n      </center>\r\n    </div>\r\n\r\n    <div class=\"col\">\r\n      <center>\r\n        <i class=\"fas fa-handshake fa-2x\"></i>\r\n        <h2>Successful Investor Engagements </h2>\r\n        <h2>(with Companies) </h2>\r\n        <h1 style=\"\">{{cohortCountInvestorsApproachesSuccessful()}}</h1>\r\n      </center>\r\n    </div>\r\n\r\n\r\n\r\n  </div>\r\n\r\n</div>",
