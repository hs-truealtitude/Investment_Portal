                "ajaxDataUrl": "",
                "isChecked": false,
                "vif": "",
                "customCode": "<div class=\"cohortData\" style=\"border-bottom: 2px solid black;\">\r\n  <center>\r\n    <h1 style=\"margin-top: 20px; text-decoration: underline;  font-size: 2.8em;  text-decoration-color: #ef4437;;\">{{$getUser('activeCohortName')}}</h1>\r\n  </center>\r\n\r\n  <div class=\"row\" style=\"margin-top: 60px; color:black; \">\r\n\r\n    <div class=\"col\">\r\n      <center>\r\n        <h1> Investors </h1>\r\n        <h1 style=\"font-size: 2.5em; color:black;\">{{cohortCountInvestors()}}</h1>\r\n      </center>\r\n    </div>\r\n\r\n    <div class=\"col\">\r\n      <center>\r\n        <h1> Companies </h1>\r\n        <h1 style=\"font-size: 2.5em;\">{{cohortCountCompanies()}}</h1>\r\n      </center>\r\n    </div>\r\n\r\n    <div v-if=\"$getUser('accountType') == 'ProgrammePartner'\" class=\"col\">\r\n      <center>\r\n        <h1> Mentors </h1>\r\n        <h1 style=\"font-size: 2.5em;\">{{cohortCountMentors()}}</h1>\r\n      </center>\r\n    </div>\r\n\r\n  </div>\r\n\r\n</div>",
                "invisible": false,
