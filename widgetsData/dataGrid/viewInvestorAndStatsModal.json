                "ajaxDataUrl": "",
                "isChecked": false,
                "vif": "",
                "customCode": "<div class=\"cohortCard\" style=\"margin-top: -50px;\" v-if=\"getCohortInvestor()\">\r\n\r\n  <div style=\"padding-left: 20px; padding-right: 20px; margin-bottom: 20px;\">\r\n    <img style=\"border-radius: 2px; width: 100px; \" class=\"profileImage\" v-if=\"getCohortInvestor().profileImage\" :src=\"$getDashboardModel('investorProfiles')[getCohortInvestor().rowKey].profileImage.url\">    </img>\r\n    <span style=\"font-weight: bold; margin-left: 20px; font-size: 30px;\">{{ getCohortInvestor().name }} {{getCohortInvestor().secondName }}</span>\r\n  </div>\r\n\r\n  <b-tabs>\r\n\r\n    <b-tab title=\"Investor Profile\" :title-link-class=\"'tab-title-class'\" active=\"\">\r\n      <div style=\"margin-top: 20px !important; \">\r\n\r\n        <div style=\"height: 20px; float: right;\" class=\"\">\r\n          <a :href=\"getCohortInvestor().linkedInProfileLink\" target=\"_blank\">\r\n            <div v-if=\"getCohortInvestor().linkedInProfileLink\" class=\"social-icon-contain\">\r\n              <i v-if=\"getCohortInvestor().linkedInProfileLink\" style=\"padding: 5px; bottom: 0;\" class=\"fab fa-linkedin\"></i>\r\n            </div>\r\n          </a>\r\n        </div>\r\n\r\n\r\n        <p style=\"font-weight: bold;\">About Me:</p>\r\n        <p>{{ getCohortInvestor().aboutMe }}</p>\r\n\r\n\r\n        <div v-if=\"getCohortInvestor().location\" class=\"row\">\r\n          <div class=\"column col-lg-4 col-sm-12 col-md-12\">\r\n            <p class=\"h4 font-weight-bold\"> Location:\r\n              <b-card-text>{{getCohortInvestor().$location$display}}</b-card-text>\r\n            </p>\r\n          </div>\r\n        </div>\r\n\r\n        <div v-if=\"getCohortInvestor().investorTypes\" class=\"column col-lg-2 col-sm-12 col-md-12\">\r\n          <p class=\"h4 font-weight-bold\"> Investor Type(s): </p>\r\n          <span style=\" font-size: 14px;\" v-for=\"item in getCohortInvestor().$investorTypes$display\">\r\n            <div class=\"rectangle\" style=\"padding: 5px; border-radius: 6px; width: 250px; margin: 10px;\">\r\n              {{item}}\r\n            </div>\r\n          </span>\r\n        </div>\r\n\r\n\r\n\r\n    </b-tab>\r\n\r\n    <b-tab title=\"Engagement Stats (within Cohort)\" :title-link-class=\"'tab-title-class'\">\r\n      <div style=\"margin-top: 20px !important;\" class=\"tabContainer\">\r\n      <div class=\"\" style=\"\">\r\n\r\n        <div class=\"row\" style=\"margin: 60px; color:black; \">\r\n\r\n          <div class=\"col\">\r\n            <center>\r\n              <i class=\"far fa-handshake fa-2x\"></i>\r\n\r\n              <h2> Investor Engagements </h2>\r\n              <h2>(with Companies)</h2>\r\n              <h1 style=\"color:black;\">{{selectedInvestorCountCompanyEngagements()}}</h1>\r\n            </center>\r\n          </div>\r\n\r\n          <div class=\"col\">\r\n            <center>\r\n              <i class=\"fas fa-handshake fa-2x\"></i>\r\n              <h2>Successful Investor Engagements </h2>\r\n              <h2>(with Companies) </h2>\r\n              <h1 style=\"\">{{cohortCountInvestorsApproachesSuccessful()}}</h1>\r\n            </center>\r\n          </div>\r\n\r\n\r\n\r\n        </div>\r\n\r\n        </div>\r\n\r\n      </div>\r\n      </div>\r\n\r\n    </b-tab>\r\n\r\n  </b-tabs>\r\n\r\n</div>\r\n",
